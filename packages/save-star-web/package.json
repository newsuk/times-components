{
  "name": "@times-components/save-star-web",
  "version": "0.10.0",
  "description": "Save Star for Web",
  "main": "dist/save-star-web",
  "dev": "src/save-star-web",
  "scripts": {
    "depcheck": "depcheck --ignores='@babel/*,babel-*,depcheck,eslint,jest,prettier,webpack*' --ignore-bin-package=false --skip-missing",
    "fmt": "prettier --write '**/*.*'",
    "prettier:diff": "prettier --list-different '**/*.*'",
    "lint": "eslint . && yarn prettier:diff && yarn depcheck",
    "test:web": "jest --config='./__tests__/jest.config.js'",
    "test:web:updatesnapshot": "yarn test:web -u",
    "prepublishOnly": "yarn transpile && yarn bundle",
    "cleanup-dist": "rm -rf dist",
    "transpile": "yarn cleanup-dist && babel src -d dist",
    "watch": "watch 'yarn bundle' ./src --ignoreDotFiles --ignoreUnreadable",
    "bundle": "NODE_ENV=production webpack -p"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/newsuk/times-components.git"
  },
  "keywords": [
    "react",
    "save-star-web",
    "component"
  ],
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/newsuk/times-components/issues"
  },
  "homepage": "https://github.com/newsuk/times-components#readme",
  "dependencies": {
    "@times-components/icons": "2.20.6",
    "@times-components/link": "3.12.5",
    "@times-components/provider": "1.35.8",
    "@times-components/provider-queries": "2.18.2",
    "@times-components/tracking": "2.19.4",
    "@times-components/ts-styleguide": "1.43.0",
    "@times-components/utils": "6.16.6",
    "prop-types": "15.7.2",
    "react-apollo": "2.5.5",
    "styled-components": "4.3.2"
  },
  "devDependencies": {
    "@babel/core": "7.4.4",
    "@times-components/eslint-config-thetimes": "0.8.18",
    "@times-components/jest-configurator-web": "0.8.1",
    "@times-components/jest-serializer": "3.8.3",
    "@times-components/provider-test-tools": "1.27.3",
    "@times-components/storybook": "4.9.3",
    "@times-components/test-utils": "2.4.1",
    "@times-components/webpack-configurator": "2.1.0",
    "babel-jest": "24.8.0",
    "babel-loader": "8.0.5",
    "eslint": "5.9.0",
    "jest": "24.8.0",
    "prettier": "1.14.3",
    "react": "16.9.0",
    "react-dom": "16.9.0",
    "react-test-renderer": "16.9.0",
    "webpack": "4.30.0"
  },
  "resolutions": {
    "react": "16.9.0",
    "react-dom": "16.9.0"
  },
  "peerDependencies": {
    "react": ">=16.9",
    "react-dom": ">=16.9"
  },
  "publishConfig": {
    "access": "public"
  }
}
