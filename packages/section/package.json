{
  "name": "@times-components/section",
  "version": "1.0.27",
  "description": "Section Page",
  "main": "dist/section",
  "dev": "src/section",
  "scripts": {
    "depcheck": "depcheck --ignores='babel-*,depcheck,eslint,jest,prettier,webpack*' --ignore-bin-package=false --skip-missing",
    "fmt": "eslint . --fix && prettier --write '**/*.*'",
    "prettier:diff": "prettier --list-different '**/*.*'",
    "lint": "eslint . && yarn prettier:diff && yarn depcheck",
    "test:android": "jest --config='./__tests__/android/jest.config.js'",
    "test:ios": "jest --config='./__tests__/ios/jest.config.js'",
    "test:unit": "jest ./__tests__/unit",
    "test:all": "yarn test:android --coverage && yarn test:ios --coverage && yarn test:unit --coverage",
    "prepublishOnly": "yarn transpile && yarn bundle",
    "cleanup-dist": "rm -rf dist",
    "transpile": "yarn cleanup-dist && babel src -d dist",
    "watch": "watch 'yarn bundle' ./src --ignoreDotFiles --ignoreUnreadable",
    "bundle": "NODE_ENV=production webpack -p"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/newsuk/times-components.git"
  },
  "keywords": [
    "react-native-web",
    "react",
    "native",
    "web",
    "section",
    "component"
  ],
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/newsuk/times-components/issues"
  },
  "homepage": "https://github.com/newsuk/times-components#readme",
  "dependencies": {
    "@times-components/context": "1.0.17",
    "@times-components/edition-slices": "0.60.9",
    "@times-components/icons": "2.11.12",
    "@times-components/image": "6.1.1",
    "@times-components/link": "3.4.16",
    "@times-components/responsive": "0.4.41",
    "@times-components/styleguide": "3.28.23",
    "@times-components/tracking": "2.4.41",
    "memoize-one": "5.0.4",
    "prop-types": "15.7.2"
  },
  "devDependencies": {
    "@times-components/eslint-config-thetimes": "0.8.12",
    "@times-components/fixture-generator": "0.25.0",
    "@times-components/jest-configurator": "2.5.12",
    "@times-components/jest-serializer": "3.2.8",
    "@times-components/star-button": "0.1.45",
    "@times-components/storybook": "4.0.10",
    "@times-components/tealium-utils": "0.7.52",
    "@times-components/webpack-configurator": "2.0.21",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "24.8.0",
    "eslint": "5.9.0",
    "jest": "24.8.0",
    "prettier": "1.14.3",
    "react": "16.6.3",
    "react-dom": "16.6.3",
    "react-native": "0.58.6",
    "react-test-renderer": "16.6.3",
    "webpack": "4.30.0",
    "webpack-cli": "3.3.1"
  },
  "peerDependencies": {
    "react": ">=16.5",
    "react-dom": ">=16.5",
    "react-native": ">=0.55",
    "react-native-web": "0.11.2"
  },
  "publishConfig": {
    "access": "public"
  }
}
