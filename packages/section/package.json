{
  "name": "@times-components/section",
  "version": "1.1.36",
  "description": "Section Page",
  "main": "dist/section",
  "dev": "src/section",
  "scripts": {
    "depcheck": "depcheck --ignores='@babel/*,babel-*,depcheck,eslint,jest,prettier,webpack*' --ignore-bin-package=false --skip-missing",
    "fmt": "eslint . --fix && prettier --write '**/*.*'",
    "prettier:diff": "prettier --list-different '**/*.*'",
    "lint": "eslint . && yarn prettier:diff && yarn depcheck",
    "test:android": "jest --config='./__tests__/android/jest.config.js'",
    "test:ios": "jest --config='./__tests__/ios/jest.config.js'",
    "test:unit": "jest --config='./__tests__/unit/jest.config.js' ./__tests__/unit",
    "test:all": "yarn test:android --coverage && yarn test:ios --coverage && yarn test:unit --coverage",
    "prepublishOnly": "yarn transpile && yarn bundle",
    "cleanup-dist": "rm -rf dist",
    "transpile": "yarn cleanup-dist && babel src -d dist",
    "watch": "watch 'yarn bundle' ./src --ignoreDotFiles --ignoreUnreadable",
    "bundle": "NODE_ENV=production webpack -p"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/newsuk/times-components.git"
  },
  "keywords": [
    "react-native-web",
    "react",
    "native",
    "web",
    "section",
    "component"
  ],
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/newsuk/times-components/issues"
  },
  "homepage": "https://github.com/newsuk/times-components#readme",
  "dependencies": {
    "@times-components/context": "1.0.32",
    "@times-components/edition-slices": "0.70.1",
    "@times-components/icons": "2.12.5",
    "@times-components/image": "6.4.12",
    "@times-components/link": "3.4.33",
    "@times-components/responsive": "0.4.56",
    "@times-components/styleguide": "3.28.38",
    "@times-components/tracking": "2.4.58",
    "memoize-one": "5.0.4",
    "prop-types": "15.7.2"
  },
  "devDependencies": {
    "@babel/core": "7.4.4",
    "@times-components/eslint-config-thetimes": "0.8.12",
    "@times-components/fixture-generator": "0.25.5",
    "@times-components/jest-configurator": "2.5.15",
    "@times-components/jest-serializer": "3.2.11",
    "@times-components/star-button": "0.1.63",
    "@times-components/storybook": "4.0.25",
    "@times-components/tealium-utils": "0.7.55",
    "@times-components/webpack-configurator": "2.0.22",
    "babel-jest": "24.8.0",
    "eslint": "5.9.0",
    "jest": "24.8.0",
    "prettier": "1.14.3",
    "react": "16.8.6",
    "react-dom": "16.8.6",
    "react-native": "0.59.10",
    "react-test-renderer": "16.6.3",
    "webpack": "4.30.0",
    "webpack-cli": "3.3.1"
  },
  "peerDependencies": {
    "react": ">=16.5",
    "react-dom": ">=16.5",
    "react-native": ">=0.55",
    "react-native-web": "0.11.4"
  },
  "publishConfig": {
    "access": "public"
  }
}
