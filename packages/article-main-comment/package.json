    "@times-components/tealium-utils": "0.7.42",
    "@times-components/test-utils": "2.2.36",
    "@times-components/webpack-configurator": "2.0.15",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "24.7.1",
