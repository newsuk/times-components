{
  "name": "@times-components/topics",
  "version": "0.3.1",
  "description": "Map of Topics related to the Article",
  "main": "dist/topics",
  "dev": "src/topics",
  "scripts": {
    "fmt": "prettier --write '**/*.*'",
    "prettier:diff": "prettier --list-different '**/*.*'",
    "lint": "eslint . && npm run prettier:diff",
    "test:android": "jest --config='./src/__tests__/android/jest.config.js'",
    "test:ios": "jest --config='./src/__tests__/ios/jest.config.js'",
    "test:web": "jest --config='./src/__tests__/web/jest.config.js'",
    "test:all": "yarn test:android --coverage & yarn test:ios --coverage & yarn test:web --coverage",
    "dextrose-stories": "dextrose generate-stories $(pwd)",
    "dextrose-clean": "dextrose clean-stories $(pwd)",
    "prepublishOnly": "yarn transpile",
    "transpile": "babel src --ignore node_modules,__tests__ -d dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/newsuk/times-components.git"
  },
  "keywords": [
    "react-native-web",
    "react",
    "native",
    "web",
    "topics",
    "component"
  ],
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/newsuk/times-components/issues"
  },
  "homepage": "https://github.com/newsuk/times-components#readme",
  "devDependencies": {
    "@storybook/react-native": "3.3.15",
    "@times-components/eslint-config-thetimes": "0.2.0",
    "@times-components/jest-configurator": "0.3.0",
    "@times-components/tealium": "0.2.18",
    "enzyme": "3.3.0",
    "eslint": "4.9.0",
    "jest": "21.2.1",
    "mockdate": "2.0.2",
    "prettier": "1.8.2",
    "react-dom": "16.2.0",
    "react-native": "0.53.3"
  },
  "dependencies": {
    "@times-components/link": "0.17.0",
    "@times-components/styleguide": "0.5.8",
    "@times-components/tracking": "0.10.9",
    "lodash.orderby": "4.6.0",
    "prop-types": "15.6.0"
  },
  "peerDependencies": {
    "react": ">=16",
    "react-dom": ">=16",
    "react-native": ">=0.50",
    "react-native-web": ">=0.1"
  },
  "publishConfig": {
    "access": "public"
  }
}
