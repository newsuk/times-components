{
  "name": "@times-components/svgs",
  "version": "2.6.5",
  "description": "Svgs wrapper",
  "main": "dist/index",
  "dev": "src/index",
  "scripts": {
    "fmt": "eslint . --fix && prettier --write '**/*.*'",
    "prettier:diff": "prettier --list-different '**/*.*'",
    "depcheck": "depcheck --ignores='@thetimes/jest-lint,babel-*,depcheck,eslint,jest,prettier,webpack*' --ignore-bin-package=false --skip-missing",
    "lint": "eslint . && yarn prettier:diff && yarn depcheck && jest-lint",
    "prepublishOnly": "yarn transpile && yarn bundle",
    "test:android": "jest --config='./__tests__/android/jest.config.js'",
    "test:ios": "jest --config='./__tests__/ios/jest.config.js'",
    "test:web": "jest --config='./__tests__/web/jest.config.js'",
    "test:all": "yarn test:android --coverage && yarn test:ios --coverage && yarn test:web --coverage",
    "dextrose-stories": "dextrose generate-stories $(pwd)",
    "dextrose-clean": "dextrose clean-stories $(pwd)",
    "cleanup-dist": "rm -rf dist",
    "transpile": "yarn cleanup-dist && babel src -d dist",
    "bundle": "NODE_ENV=production webpack -p"
  },
  "jest": {
    "preset": "react-native",
    "rootDir": "../../",
    "coverageDirectory": "<rootDir>/packages/svgs/coverage/",
    "transformIgnorePatterns": [
      "node_modules/(?!(react-native|react-native-svg|@times-components)/)"
    ],
    "testMatch": [
      "<rootDir>/packages/svgs/__tests__/*.test.js"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/newsuk/times-components.git"
  },
  "keywords": [
    "react-native-web",
    "react",
    "native",
    "web",
    "svgs",
    "component"
  ],
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/newsuk/times-components/issues"
  },
  "homepage": "https://github.com/newsuk/times-components#readme",
  "devDependencies": {
    "@thetimes/jest-lint": "*",
    "@times-components/eslint-config-thetimes": "0.8.9",
    "@times-components/jest-configurator": "2.4.5",
    "@times-components/jest-serializer": "3.2.0",
    "@times-components/test-utils": "2.2.25",
    "@times-components/webpack-configurator": "2.0.15",
    "babel-cli": "6.26.0",
    "babel-core": "6.26.0",
    "babel-loader": "7.1.4",
    "babel-plugin-add-react-displayname": "0.0.5",
    "babel-plugin-styled-components": "1.5.1",
    "babel-preset-react-native": "4.0.0",
    "depcheck": "0.6.9",
    "eslint": "5.9.0",
    "jest": "23.3.0",
    "prettier": "1.14.3",
    "react": "16.5.2",
    "react-dom": "16.5.2",
    "react-native": "0.55.4",
    "react-test-renderer": "16.5.2",
    "webpack": "4.6.0",
    "webpack-cli": "2.1.4"
  },
  "dependencies": {
    "prop-types": "15.6.2",
    "react-native-svg": "6.5.3",
    "svgs": "3.2.1"
  },
  "peerDependencies": {
    "react": ">=16.5",
    "react-dom": ">=16.5",
    "react-native": ">=0.55",
    "react-native-web": "0.9.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
