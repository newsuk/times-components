{
  "preset": "react-native",
  "rootDir": "../../",
  "coverageDirectory": "<rootDir>/packages/article-summary/coverage/native/",
  "collectCoverageFrom": [
    "**/packages/article-summary/*.js",
    "!**/packages/article-summary/__tests__",
    "!**/packages/article-summary/*.stories.js"
  ],
  "transformIgnorePatterns": [
    "node_modules/(?!(react-native|@times-components)/)"
  ],
  "testMatch": [
    "<rootDir>/packages/article-summary/__tests__/**!(.web).test.js"
  ]
}
