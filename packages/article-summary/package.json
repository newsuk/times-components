{
  "name": "@times-components/article-summary",
  "version": "0.22.6",
  "main": "dist/article-summary",
  "dev": "src/article-summary",
  "scripts": {
    "fmt": "prettier --write '**/*.*'",
    "prettier:diff": "prettier --list-different '**/*.*'",
    "lint": "eslint . && yarn prettier:diff",
    "test:android": "jest --config='./src/__tests__/android/jest.config.js'",
    "test:ios": "jest --config='./src/__tests__/ios/jest.config.js'",
    "test:web": "jest --config='./src/__tests__/web/jest.config.js'",
    "test:all": "yarn test:android --coverage & yarn test:ios --coverage & yarn test:web --coverage",
    "dextrose-stories": "dextrose generate-stories $(pwd)",
    "dextrose-clean": "dextrose clean-stories $(pwd)",
    "prepublishOnly": "yarn transpile",
    "transpile": "babel src --ignore node_modules,__tests__ -d dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/newsuk/times-components.git"
  },
  "keywords": [
    "react-native-web",
    "react",
    "native",
    "web",
    "article-summary",
    "component"
  ],
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/newsuk/times-components/issues"
  },
  "homepage": "https://github.com/newsuk/times-components#readme",
  "devDependencies": {
    "@storybook/react-native": "3.3.15",
    "@times-components/eslint-config-thetimes": "0.1.7",
    "dextrose": "1.7.6",
    "@times-components/jest-configurator": "0.2.5",
    "eslint": "4.9.0",
    "jest": "21.2.1",
    "prettier": "1.8.2",
    "react": "16.2.0",
    "react-dom": "16.2.0",
    "react-native": "0.53.3",
    "react-native-web": "0.3.2",
    "react-test-renderer": "16.2.0"
  },
  "dependencies": {
    "@times-components/article-byline": "0.14.9",
    "@times-components/article-label": "0.9.9",
    "@times-components/date-publication": "0.14.6",
    "@times-components/markup": "0.25.12",
    "@times-components/responsive-styles": "0.6.3",
    "@times-components/styleguide": "0.5.7",
    "date-fns": "1.28.5",
    "prop-types": "15.6.0"
  },
  "peerDependencies": {
    "babel-preset-env": "1.6.1",
    "babel-preset-stage-2": "6.24.1",
    "react": ">=16",
    "react-dom": ">=16",
    "react-native": ">=0.50",
    "react-native-web": ">=0.3"
  },
  "publishConfig": {
    "access": "public"
  }
}
