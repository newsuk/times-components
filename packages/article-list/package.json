    "@times-components/storybook": "4.7.0",
    "@times-components/tealium-utils": "0.8.2",
    "@times-components/test-utils": "2.4.0",
    "@times-components/webpack-configurator": "2.1.0",
    "apollo-client": "2.5.1",
    "babel-jest": "24.8.0",
    "babel-loader": "8.0.5",
    "depcheck": "0.6.9",
    "enzyme": "3.9.0",
    "eslint": "5.9.0",
    "graphql": "14.2.1",
