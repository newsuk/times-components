{
  "name": "@times-components/article-list",
  "version": "3.0.24",
  "description": "Paginated list of articles",
  "main": "dist/article-list",
  "dev": "src/article-list",
  "scripts": {
    "fmt": "eslint . --fix && prettier --write '**/*.*'",
    "prettier:diff": "prettier --list-different '**/*.*'",
    "depcheck": "depcheck --ignores='babel-*,depcheck,eslint,graphql,jest,prettier,webpack*' --ignore-bin-package=false --skip-missing",
    "lint": "eslint . && yarn prettier:diff && yarn depcheck",
    "test:android": "jest --config='./__tests__/android/jest.config.js'",
    "test:ios": "jest --config='./__tests__/ios/jest.config.js'",
    "test:web": "jest --config='./__tests__/web/jest.config.js' --runInBand",
    "test:all": "yarn test:android --coverage & yarn test:ios --coverage & yarn test:web --coverage",
    "prepublishOnly": "yarn transpile && yarn bundle",
    "cleanup-dist": "rm -rf dist",
    "transpile": "yarn cleanup-dist && babel src -d dist",
    "bundle": "NODE_ENV=production webpack -p"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/newsuk/times-components.git"
  },
  "keywords": [
    "react-native-web",
    "react",
    "native",
    "web",
    "article-list",
    "component"
  ],
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/newsuk/times-components/issues"
  },
  "homepage": "https://github.com/newsuk/times-components#readme",
  "devDependencies": {
    "@times-components/eslint-config-thetimes": "0.7.0",
    "@times-components/jest-configurator": "2.0.6",
    "@times-components/jest-serializer": "2.0.6",
    "@times-components/provider-test-tools": "0.15.7",
    "@times-components/storybook": "2.1.0",
    "@times-components/tealium-utils": "0.5.9",
    "@times-components/test-utils": "0.2.6",
    "@times-components/webpack-configurator": "2.0.1",
    "apollo-client": "2.3.2",
    "babel-cli": "6.26.0",
    "babel-loader": "7.1.4",
    "babel-plugin-react-native-web": "0.6.1",
    "babel-preset-react-native": "4.0.0",
    "depcheck": "0.6.9",
    "enzyme": "3.3.0",
    "eslint": "4.9.0",
    "graphql": "0.13.1",
    "jest": "23.3.0",
    "jest-styled-components": "5.0.0",
    "prettier": "1.8.2",
    "react": "16.4.2",
    "react-dom": "16.4.2",
    "react-native": "0.55.4",
    "react-native-web": "0.5.1",
    "react-test-renderer": "16.4.2",
    "webpack": "4.6.0",
    "webpack-cli": "2.1.4"
  },
  "dependencies": {
    "@times-components/ad": "2.0.15",
    "@times-components/article-summary": "2.0.22",
    "@times-components/button": "2.0.12",
    "@times-components/card": "2.0.14",
    "@times-components/error-view": "2.0.11",
    "@times-components/image": "3.0.13",
    "@times-components/link": "2.0.12",
    "@times-components/pagination": "2.0.12",
    "@times-components/responsive-styles": "2.0.10",
    "@times-components/styleguide": "2.1.8",
    "@times-components/tracking": "2.0.10",
    "@times-components/utils": "2.1.6",
    "@times-components/watermark": "2.0.12",
    "lodash.get": "4.4.2",
    "prop-types": "15.6.2"
  },
  "peerDependencies": {
    "react": ">=16",
    "react-dom": ">=16",
    "react-native": ">=0.50",
    "react-native-web": ">=0.3"
  },
