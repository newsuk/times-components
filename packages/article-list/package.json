{
  "name": "@times-components/article-list",
  "version": "6.1.1",
  "description": "Paginated list of articles",
  "main": "dist/article-list",
  "dev": "src/article-list",
  "scripts": {
    "fmt": "eslint . --fix && prettier --write '**/*.*'",
    "prettier:diff": "prettier --list-different '**/*.*'",
    "depcheck": "depcheck --ignores='@thetimes/jest-lint,babel-*,depcheck,eslint,graphql,jest,prettier,react-art,stylelint*,webpack*' --ignore-bin-package=false --skip-missing",
    "lint": "eslint . && stylelint . && yarn prettier:diff && yarn depcheck && jest-lint",
    "test:android": "jest --config='./__tests__/android/jest.config.js'",
    "test:ios": "jest --config='./__tests__/ios/jest.config.js'",
    "test:web": "jest --config='./__tests__/web/jest.config.js' --runInBand",
    "test:all": "yarn test:android --coverage && yarn test:ios --coverage && yarn test:web --coverage",
    "prepublishOnly": "yarn transpile && yarn bundle",
    "cleanup-dist": "rm -rf dist",
    "transpile": "yarn cleanup-dist && babel src -d dist",
    "bundle": "NODE_ENV=production webpack -p"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/newsuk/times-components.git"
  },
  "keywords": [
    "react-native-web",
    "react",
    "native",
    "web",
    "article-list",
    "component"
  ],
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/newsuk/times-components/issues"
  },
  "homepage": "https://github.com/newsuk/times-components#readme",
  "devDependencies": {
    "@thetimes/jest-lint": "*",
    "@times-components/eslint-config-thetimes": "0.8.8",
    "@times-components/jest-configurator": "2.1.17",
    "@times-components/jest-serializer": "3.1.10",
    "@times-components/provider": "1.8.4",
    "@times-components/provider-queries": "1.8.5",
    "@times-components/provider-test-tools": "1.10.3",
    "@times-components/storybook": "3.1.26",
    "@times-components/tealium-utils": "0.7.8",
    "@times-components/test-utils": "2.0.3",
    "@times-components/webpack-configurator": "2.0.14",
    "apollo-client": "2.3.7",
    "babel-cli": "6.26.0",
    "babel-loader": "7.1.4",
    "babel-preset-react-native": "4.0.0",
    "depcheck": "0.6.9",
