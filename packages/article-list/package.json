{
  "name": "@times-components/article-list",
  "version": "1.1.5",
  "description": "Paginated list of articles",
  "main": "dist/article-list",
  "dev": "src/article-list",
  "scripts": {
    "fmt": "prettier --write '**/*.*'",
    "prettier:diff": "prettier --list-different '**/*.*'",
    "lint": "eslint . && npm run prettier:diff",
    "test:android": "jest --config='./__tests__/android/jest.config.js'",
    "test:ios": "jest --config='./__tests__/ios/jest.config.js'",
    "test:web": "jest --config='./__tests__/web/jest.config.js'",
    "test:all": "yarn test:android --coverage & yarn test:ios --coverage & yarn test:web --coverage",
    "prepublishOnly": "npm run transpile",
    "cleanup-dist": "rm -rf dist",
    "transpile": "yarn cleanup-dist && babel src -d dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/newsuk/times-components.git"
  },
  "keywords": [
    "react-native-web",
    "react",
    "native",
    "web",
    "article-list",
    "component"
  ],
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/newsuk/times-components/issues"
  },
  "homepage": "https://github.com/newsuk/times-components#readme",
  "devDependencies": {
    "@storybook/react-native": "3.4.1",
    "@times-components/eslint-config-thetimes": "0.4.1",
    "@times-components/jest-configurator": "0.7.0",
    "@times-components/provider-test-tools": "0.5.4",
    "@times-components/storybook": "0.10.2",
    "@times-components/tealium-utils": "0.3.1",
    "babel-cli": "6.26.0",
    "babel-preset-react-native": "4.0.0",
    "enzyme": "3.3.0",
    "enzyme-adapter-react-16": "1.1.0",
    "enzyme-to-json": "3.3.0",
    "eslint": "4.9.0",
    "jest": "21.2.1",
    "lodash.clonedeep": "4.5.0",
    "lodash.set": "4.3.2",
    "prettier": "1.8.2",
    "react": "16.3.1",
    "react-dom": "16.3.1",
    "react-native": "0.54.2",
    "react-native-web": "0.5.1",
    "react-test-renderer": "16.3.1"
  },
  "dependencies": {
    "@times-components/article-summary": "0.24.2",
    "@times-components/card": "0.29.2",
    "@times-components/error-view": "0.11.13",
    "@times-components/image": "1.18.2",
    "@times-components/link": "0.21.2",
    "@times-components/pagination": "0.19.2",
    "@times-components/responsive-styles": "0.8.2",
    "@times-components/styleguide": "0.7.3",
    "@times-components/tracking": "0.12.16",
    "@times-components/utils": "0.10.1",
    "lodash.get": "4.4.2",
    "prop-types": "15.6.0"
  },
  "peerDependencies": {
    "react": ">=16",
    "react-dom": ">=16",
    "react-native": ">=0.50",
    "react-native-web": ">=0.3"
  },
  "publishConfig": {
    "access": "public"
  }
}
