{
  "name": "@times-components/sticky",
  "version": "0.2.22",
  "description": "Attach children to viewport on scroll (web only)",
  "main": "dist/sticky",
  "dev": "src/sticky",
  "scripts": {
    "depcheck": "depcheck --ignores='@babel/*,babel-*,depcheck,eslint,stylelint*,jest,prettier,webpack*' --ignore-bin-package=false --skip-missing",
    "fmt": "eslint . --fix && prettier --write '**/*.*'",
    "prettier:diff": "prettier --list-different '**/*.*'",
    "lint": "eslint . && stylelint . && yarn prettier:diff && yarn depcheck",
    "test:web": "jest --config='./__tests__/web/jest.config.js'",
    "test:all": "yarn test:web --coverage",
    "prepublishOnly": "yarn transpile && yarn bundle",
    "cleanup-dist": "rm -rf dist",
    "transpile": "yarn cleanup-dist && babel src -d dist",
    "watch": "watch 'yarn bundle' ./src --ignoreDotFiles --ignoreUnreadable",
    "bundle": "NODE_ENV=production webpack -p"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/newsuk/times-components.git"
  },
  "keywords": [
    "react-native-web",
    "react",
    "native",
    "web",
    "sticky",
    "component"
  ],
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/newsuk/times-components/issues"
  },
  "homepage": "https://github.com/newsuk/times-components#readme",
  "dependencies": {
    "hoist-non-react-statics": "2.5.5",
    "prop-types": "15.7.2",
    "react-display-name": "0.2.3",
    "styled-components": "4.3.2"
  },
  "devDependencies": {
    "@babel/core": "7.4.4",
    "@times-components/eslint-config-thetimes": "0.8.16",
    "@times-components/jest-configurator": "2.6.10",
    "@times-components/storybook": "4.1.29",
    "@times-components/styleguide": "3.34.2",
    "@times-components/webpack-configurator": "2.0.27",
    "babel-jest": "24.8.0",
    "babel-loader": "8.0.5",
    "eslint": "5.9.0",
    "jest": "24.8.0",
    "prettier": "1.14.3",
    "react": "16.9.0",
    "react-dom": "16.9.0",
    "react-native": "0.61.1",
    "stylelint": "9.4.0",
    "stylelint-config-recommended": "2.1.0",
    "stylelint-config-styled-components": "0.1.1",
    "stylelint-processor-styled-components": "1.3.2",
    "webpack": "4.30.0",
    "webpack-cli": "3.3.1"
  },
  "resolutions": {
    "react": "16.9.0",
    "react-dom": "16.9.0"
  },
  "peerDependencies": {
    "react": ">=16.9",
    "react-dom": ">=16.9",
    "react-native": ">=0.59",
    "react-native-web": "0.11.4"
  },
  "publishConfig": {
    "access": "public"
  }
}
