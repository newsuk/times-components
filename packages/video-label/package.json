{
  "name": "@times-components/video-label",
  "version": "0.1.24",
  "description": "A label for articles with videos",
  "main": "dist/video-label",
  "dev": "src/video-label",
  "scripts": {
    "fmt": "prettier --write '**/*.*'",
    "prettier:diff": "prettier --list-different '**/*.*'",
    "lint": "eslint . && yarn prettier:diff",
    "test:android": "jest --config='./src/__tests__/android/jest.config.js'",
    "test:ios": "jest --config='./src/__tests__/ios/jest.config.js'",
    "test:web": "jest --config='./src/__tests__/web/jest.config.js'",
    "test:all": "yarn test:android --coverage & yarn test:ios --coverage & yarn test:web --coverage",
    "prepublishOnly": "yarn transpile",
    "transpile": "babel src --ignore node_modules,__tests__ -d dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/newsuk/times-components.git"
  },
  "keywords": [
    "react-native-web",
    "react",
    "native",
    "web",
    "video-label",
    "component"
  ],
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/newsuk/times-components/issues"
  },
  "homepage": "https://github.com/newsuk/times-components#readme",
  "dependencies": {
    "@times-components/icons": "0.4.0",
    "@times-components/styleguide": "0.5.8",
    "lodash.invert": "4.3.0",
    "prop-types": "15.6.0"
  },
  "devDependencies": {
    "@storybook/addon-knobs": "3.3.15",
    "@storybook/react-native": "3.3.15",
    "@times-components/eslint-config-thetimes": "0.2.0",
    "@times-components/jest-configurator": "0.3.0",
    "@times-components/storybook": "0.5.0",
    "babel-plugin-flow-react-proptypes": "22.0.0",
    "babel-plugin-add-react-displayname": "0.0.5",
    "babel-plugin-styled-components": "1.5.1",
    "babel-preset-react-native": "4.0.0",
    "dextrose": "1.7.6",
    "enzyme": "3.3.0",
    "enzyme-to-json": "3.3.0",
    "eslint": "4.9.0",
    "jest": "21.2.1",
    "prettier": "1.8.2",
    "react-test-renderer": "16.2.0"
  },
  "peerDependencies": {
    "react": ">=16",
    "react-dom": ">=16",
    "react-native": ">=0.50",
    "react-native-web": ">=0.3"
  },
  "publishConfig": {
    "access": "public"
  }
}
