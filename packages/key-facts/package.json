    "babel-jest": "24.8.0",
    "depcheck": "0.6.9",
    "enzyme": "3.9.0",
    "eslint": "5.9.0",
    "jest": "24.8.0",
    "mockdate": "3.0.5",
    "prettier": "1.14.3",
    "react": "16.9.0",
    "react-art": "16.6.3",
    "react-dom": "16.9.0",
    "stylelint": "9.4.0",
    "stylelint-config-recommended": "2.1.0",
    "stylelint-config-styled-components": "0.1.1",
    "stylelint-processor-styled-components": "1.3.2",
    "webpack": "4.30.0",
