{
  "name": "@times-components/key-facts",
  "version": "1.2.7",
  "description": "Bulleted list of text data",
  "main": "dist/key-facts",
  "dev": "src/key-facts",
  "scripts": {
    "fmt": "eslint . --fix && prettier --write '**/*.*'",
    "prettier:diff": "prettier --list-different '**/*.*'",
    "depcheck": "depcheck --ignores='@thetimes/jest-lint,babel-*,depcheck,eslint,jest,prettier,stylelint*,webpack*' --ignore-bin-package=false --skip-missing",
    "lint": "eslint . && stylelint . && yarn prettier:diff && yarn depcheck && jest-lint",
    "test:android": "jest --config='./__tests__/android/jest.config.js'",
    "test:ios": "jest --config='./__tests__/ios/jest.config.js'",
    "test:web": "jest --config='./__tests__/web/jest.config.js'",
    "test:all": "yarn test:android --coverage && yarn test:ios --coverage && yarn test:web --coverage",
    "prepublishOnly": "yarn transpile && yarn bundle",
    "cleanup-dist": "rm -rf dist",
    "transpile": "yarn cleanup-dist && babel src -d dist",
    "bundle": "NODE_ENV=production webpack -p"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/newsuk/times-components.git"
  },
  "keywords": [
    "react-native-web",
    "react",
    "native",
    "web",
    "key-facts",
    "component"
  ],
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/newsuk/times-components/issues"
  },
  "homepage": "https://github.com/newsuk/times-components#readme",
  "devDependencies": {
    "@thetimes/jest-lint": "*",
    "@times-components/eslint-config-thetimes": "0.7.1",
    "@times-components/jest-configurator": "2.1.4",
    "@times-components/jest-serializer": "3.1.0",
    "@times-components/storybook": "3.0.8",
    "@times-components/test-utils": "1.0.2",
    "@times-components/webpack-configurator": "2.0.4",
    "babel-cli": "6.26.0",
    "depcheck": "0.6.9",
    "enzyme": "3.3.0",
    "eslint": "4.9.0",
    "jest": "23.3.0",
    "jest-styled-components": "5.0.1",
    "prettier": "1.8.2",
    "react": "16.4.2",
    "react-dom": "16.4.2",
    "react-native": "0.55.4",
    "react-test-renderer": "16.4.2",
    "stylelint": "9.4.0",
    "stylelint-config-recommended": "2.1.0",
    "stylelint-config-styled-components": "0.1.1",
    "stylelint-processor-styled-components": "1.3.2",
    "webpack": "4.6.0",
    "webpack-cli": "2.1.4"
  },
  "dependencies": {
    "@times-components/context": "0.4.7",
    "@times-components/link": "3.0.14",
    "@times-components/markup": "3.0.13",
    "@times-components/markup-forest": "1.0.2",
    "@times-components/styleguide": "3.2.6",
    "lodash.invert": "4.3.0",
    "prop-types": "15.6.2",
    "styled-components": "3.4.0"
  },
  "peerDependencies": {
    "react": ">=16",
    "react-dom": ">=16",
    "react-native": ">=0.50",
    "react-native-web": ">=0.3"
  },
  "publishConfig": {
    "access": "public"
  }
}
