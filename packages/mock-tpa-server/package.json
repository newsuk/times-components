{
  "name": "@times-components/mock-tpa-server",
  "version": "0.1.15",
  "description": "utility for testing times component clients with mocked api",
  "main": "./dist/index",
  "scripts": {
    "fmt": "eslint . --fix && prettier --write '**/*.*'",
    "prettier:diff": "prettier --list-different '**/*.*'",
    "depcheck": "depcheck --ignores='babel-*,depcheck,**/eslint*,graphql,jest,prettier,webpack*' --ignore-bin-package=false --skip-missing",
    "lint": "eslint . && yarn prettier:diff && yarn depcheck",
    "transpile": "babel src -d dist --copy-files"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/newsuk/times-components.git"
  },
  "keywords": [
    "react",
    "native",
    "web",
    "graphql",
    "provider",
    "component"
  ],
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/newsuk/times-components/issues"
  },
  "homepage": "https://github.com/newsuk/times-components#readme",
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/preset-env": "7.1.6",
    "@times-components/eslint-config-thetimes": "0.8.8",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^23.4.2",
    "depcheck": "0.6.9",
    "eslint": "5.9.0",
    "eslint-plugin-graphql": "2.1.1",
    "jest": "23.3.0",
    "prettier": "1.14.3"
  },
  "dependencies": {
    "@times-components/provider-test-tools": "1.10.3",
    "@times-components/schema": "0.1.15",
    "apollo-server": "2.1.0",
    "graphql": "0.13.2"
  },
  "publishConfig": {
    "access": "public"
  }
}
