{
  "name": "@times-components/edition-slices",
  "version": "0.60.0",
  "description": "Components used in edition pages including slices and tiles",
  "main": "dist/edition-slices",
  "dev": "src/edition-slices",
  "scripts": {
    "depcheck": "depcheck --ignores='babel-*,depcheck,eslint,jest,prettier,webpack*' --ignore-bin-package=false --skip-missing",
    "fmt": "eslint . --fix && prettier --write '**/*.*'",
    "prettier:diff": "prettier --list-different '**/*.*'",
    "lint": "eslint . && yarn prettier:diff && yarn depcheck",
    "test:android": "jest --config='./__tests__/android/jest.config.js'",
    "test:ios": "jest --config='./__tests__/ios/jest.config.js'",
    "test:web": "jest --config='./__tests__/web/jest.config.js'",
    "test:all": "yarn test:android --coverage && yarn test:ios --coverage && yarn test:web --coverage",
    "prepublishOnly": "yarn transpile && yarn bundle",
    "cleanup-dist": "rm -rf dist",
    "transpile": "yarn cleanup-dist && babel src -d dist",
    "bundle": "NODE_ENV=production webpack -p"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/newsuk/times-components.git"
  },
  "keywords": [
    "react-native-web",
    "react",
    "native",
    "web",
    "edition-slices",
    "component"
  ],
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/newsuk/times-components/issues"
  },
  "homepage": "https://github.com/newsuk/times-components#readme",
  "dependencies": {
    "@times-components/article-flag": "2.6.40",
    "@times-components/article-summary": "3.10.50",
    "@times-components/context": "1.0.13",
    "@times-components/icons": "2.11.9",
    "@times-components/image": "6.0.12",
    "@times-components/link": "3.4.13",
    "@times-components/markup": "3.3.68",
    "@times-components/markup-forest": "1.7.12",
    "@times-components/responsive": "0.4.38",
    "@times-components/slice-layout": "0.29.5",
    "@times-components/star-button": "0.1.42",
    "@times-components/styleguide": "3.28.20",
    "@times-components/tracking": "2.4.39",
    "prop-types": "15.7.2"
  },
  "devDependencies": {
    "@times-components/eslint-config-thetimes": "0.8.12",
    "@times-components/fixture-generator": "0.25.0",
    "@times-components/jest-configurator": "2.5.12",
    "@times-components/jest-serializer": "3.2.8",
    "@times-components/mocks": "0.0.15",
    "@times-components/storybook": "4.0.9",
    "@times-components/test-utils": "2.2.43",
    "@times-components/webpack-configurator": "2.0.21",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "24.8.0",
    "eslint": "5.9.0",
    "jest": "24.8.0",
    "prettier": "1.14.3",
    "react": "16.6.3",
    "react-dom": "16.6.3",
    "react-native": "0.58.6",
    "react-test-renderer": "16.6.3",
    "webpack": "4.30.0",
    "webpack-cli": "3.3.1"
  },
  "peerDependencies": {
    "react": ">=16.5",
    "react-dom": ">=16.5",
    "react-native": ">=0.55",
    "react-native-web": "0.11.2"
  },
  "publishConfig": {
    "access": "public"
  }
}
