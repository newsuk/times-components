{
  "name": "@times-components/provider",
  "version": "0.23.7",
  "main": "provider",
  "scripts": {
    "prettier:diff": "prettier --list-different '**/*.*'",
    "fmt": "prettier --write '**/*.*'",
    "lint": "eslint . && yarn prettier:diff",
    "test": "jest",
    "dextrose-stories": "dextrose generate-stories $(pwd)",
    "dextrose-clean": "dextrose clean-stories $(pwd)"
  },
  "jest": {
    "preset": "react-native",
    "rootDir": "../../",
    "testEnvironment": "jsdom",
    "coverageDirectory": "<rootDir>/packages/provider/coverage/",
    "collectCoverageFrom": [
      "**/packages/provider/*.js",
      "!**/packages/provider/*stories.js"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!(react-native|@times-components)/)"
    ],
    "testMatch": [
      "<rootDir>/packages/provider/__tests__/**.test.js"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/newsuk/times-components.git"
  },
  "keywords": [
    "react-native-web",
    "react",
    "native",
    "web",
    "provider",
    "component"
  ],
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/newsuk/times-components/issues"
  },
  "homepage": "https://github.com/newsuk/times-components#readme",
  "devDependencies": {
    "@storybook/react-native": "3.3.15",
    "@times-components/eslint-config-thetimes": "0.1.3",
    "@times-components/provider-test-tools": "0.1.11",
    "@times-components/utils": "0.5.3",
    "apollo-utilities": "1.0.4",
    "dextrose": "1.5.0",
    "enzyme": "3.3.0",
    "enzyme-adapter-react-16": "1.1.0",
    "eslint": "4.9.0",
    "eslint-plugin-graphql": "1.5.0",
    "graphql": "0.12.3",
    "jest": "21.2.1",
    "prettier": "1.8.2",
    "react": "16.2.0",
    "react-dom": "16.2.0",
    "react-native": "0.53.3",
    "react-test-renderer": "16.2.0"
  },
  "dependencies": {
    "graphql-tag": "2.6.0",
    "hoist-non-react-statics": "2.3.1",
    "lodash.get": "4.4.2",
    "lodash.isequal": "4.4.0",
    "lodash.pick": "4.4.0",
    "prop-types": "15.6.0",
    "react-apollo": "2.1.0-rc.3",
    "react-display-name": "0.2.3"
  },
  "peerDependencies": {
    "babel-preset-env": "1.6.1",
    "babel-preset-stage-2": "6.24.1",
    "react": ">=16",
    "react-dom": ">=16",
    "react-native": ">=0.50",
    "react-native-web": ">=0.3"
  },
  "publishConfig": {
    "access": "public"
  }
}
