{
  "name": "@times-components/provider",
  "version": "0.20.11",
  "main": "provider",
  "scripts": {
    "test:watch": "jest --bail --verbose --watchAll",
    "test": "jest --bail --ci --coverage",
    "prettier:diff": "prettier --list-different '**/*.*'",
    "fmt": "prettier --write '**/*.*'",
    "lint": "eslint . && yarn prettier:diff",
    "dextrose-stories": "dextrose-stories",
    "dextrose-clean": "dextrose-clean"
  },
  "jest": {
    "preset": "react-native",
    "rootDir": "../../",
    "coverageDirectory": "<rootDir>/packages/provider/coverage/",
    "collectCoverageFrom": [
      "**/packages/provider/*.js",
      "!**/packages/provider/*stories.js"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!(react-native|react-apollo-temp|@times-components)/)"
    ],
    "testMatch": [
      "<rootDir>/packages/provider/__tests__/**.test.js"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/newsuk/times-components.git"
  },
  "keywords": [
    "react-native-web",
    "react",
    "native",
    "web",
    "provider",
    "component"
  ],
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/newsuk/times-components/issues"
  },
  "homepage": "https://github.com/newsuk/times-components#readme",
  "devDependencies": {
    "@storybook/react-native": "3.2.18",
    "@times-components/eslint-config-thetimes": "0.1.2",
    "@times-components/utils": "0.1.2",
    "apollo-utilities": "1.0.4",
    "dextrose": "1.2.14",
    "enzyme": "3.2.0",
    "enzyme-adapter-react-16": "1.1.0",
    "eslint": "4.9.0",
    "eslint-plugin-graphql": "1.4.1",
    "graphql": "0.11.7",
    "jest": "21.2.1",
    "prettier": "1.8.2",
    "react": "16.2.0",
    "react-dom": "16.2.0",
    "react-native": "0.50.1",
    "react-test-renderer": "16.2.0"
  },
  "dependencies": {
    "graphql-tag": "2.5.0",
    "lodash.get": "4.4.2",
    "lodash.pick": "4.4.0",
    "prop-types": "15.6.0",
    "react-apollo-temp": "2.0.5"
  },
  "peerDependencies": {
    "react": ">=16",
    "react-dom": ">=16",
    "react-native": ">=0.50",
    "react-native-web": ">=0.1"
  },
  "publishConfig": {
    "access": "public"
  }
}
