    "babel-plugin-styled-components": "1.5.1",
    "babel-preset-react-native": "4.0.0",
    "enzyme": "3.3.0",
    "enzyme-adapter-react-16": "1.1.0",
    "eslint": "4.9.0",
    "eslint-plugin-graphql": "1.5.0",
    "graphql": "0.13.1",
    "jest": "21.2.1",
    "prettier": "1.8.2",
    "react": "16.3.1",
    "react-dom": "16.3.1",
    "react-native": "0.54.2",
    "react-test-renderer": "16.3.1",
