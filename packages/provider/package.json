{
  "name": "@times-components/provider",
  "version": "0.28.4",
  "main": "dist/provider",
  "dev": "src/provider",
  "scripts": {
    "prettier:diff": "prettier --list-different '**/*.*'",
    "fmt": "prettier --write '**/*.*'",
    "lint": "eslint . && yarn prettier:diff",
    "test": "jest",
    "dextrose-stories": "dextrose generate-stories $(pwd)",
    "dextrose-clean": "dextrose clean-stories $(pwd)",
    "prepublishOnly": "yarn transpile",
    "cleanup-dist": "rm -rf dist",
    "transpile": "yarn cleanup-dist && babel src -d dist"
  },
  "jest": {
    "preset": "react-native",
    "rootDir": "../../",
    "testEnvironment": "jsdom",
    "coverageDirectory": "<rootDir>/packages/provider/coverage/",
    "collectCoverageFrom": [
      "**/packages/provider/*.js",
      "!**/packages/provider/*stories.js"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!(react-native|@times-components)/)"
    ],
    "testMatch": [
      "<rootDir>/packages/provider/__tests__/**.test.js"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/newsuk/times-components.git"
  },
  "keywords": [
    "react-native-web",
    "react",
    "native",
    "web",
    "provider",
    "component"
  ],
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/newsuk/times-components/issues"
  },
  "homepage": "https://github.com/newsuk/times-components#readme",
  "devDependencies": {
    "@times-components/eslint-config-thetimes": "0.4.1",
    "@times-components/provider-test-tools": "0.3.4",
    "@times-components/storybook": "0.9.3",
    "@times-components/utils": "0.8.1",
    "apollo-utilities": "1.0.4",
    "babel-core": "6.26.0",
    "babel-plugin-add-react-displayname": "0.0.5",
    "babel-plugin-flow-react-proptypes": "22.0.0",
    "babel-plugin-styled-components": "1.5.1",
    "babel-preset-react-native": "4.0.0",
    "dextrose": "1.7.6",
    "enzyme": "3.3.0",
    "enzyme-adapter-react-16": "1.1.0",
    "eslint": "4.9.0",
    "eslint-plugin-graphql": "1.5.0",
    "graphql": "0.12.3",
    "jest": "21.2.1",
    "prettier": "1.8.2",
    "react": "16.3.1",
    "react-dom": "16.3.1",
    "react-native": "0.54.2",
    "react-test-renderer": "16.3.1"
  },
  "dependencies": {
    "graphql-tag": "2.8.0",
    "hoist-non-react-statics": "2.3.1",
    "lodash.isequal": "4.4.0",
    "lodash.pick": "4.4.0",
    "prop-types": "15.6.0",
    "react-apollo": "2.1.0-rc.3",
    "react-display-name": "0.2.3"
  },
  "peerDependencies": {
    "react": ">=16.2",
    "react-dom": ">=16.2",
    "react-native": ">=0.54",
    "react-native-web": ">=0.3"
  },
  "publishConfig": {
    "access": "public"
  }
}
