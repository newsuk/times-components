{
  "name": "@times-components/provider",
  "version": "1.17.5",
  "main": "dist/provider",
  "dev": "src/provider",
  "scripts": {
    "prettier:diff": "prettier --list-different '**/*.*'",
    "fmt": "eslint . --fix && prettier --write '**/*.*'",
    "depcheck": "depcheck --ignores='babel-*,depcheck,**/eslint*,graphql,jest,prettier,webpack*' --ignore-bin-package=false --skip-missing",
    "lint": "eslint . && yarn prettier:diff && yarn depcheck",
    "test:web": "jest --config='./__tests__/web/jest.config.js'",
    "test": "yarn test:web && jest --config='./__tests__/jest.config.js'",
    "prepublishOnly": "yarn transpile && yarn bundle",
    "watch": "watch 'yarn bundle' ./src --ignoreDotFiles --ignoreUnreadable",
    "cleanup-dist": "rm -rf dist",
    "transpile": "yarn cleanup-dist && babel src -d dist",
    "bundle": "NODE_ENV=production webpack -p"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/newsuk/times-components.git"
  },
  "keywords": [
    "react-native-web",
    "react",
    "native",
    "web",
    "provider",
    "component"
  ],
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/newsuk/times-components/issues"
  },
  "homepage": "https://github.com/newsuk/times-components#readme",
  "devDependencies": {
    "@times-components/eslint-config-thetimes": "0.8.12",
    "@times-components/jest-configurator": "2.5.12",
    "@times-components/jest-serializer": "3.2.8",
    "@times-components/provider-test-tools": "1.17.19",
    "@times-components/storybook": "4.0.9",
    "@times-components/test-utils": "2.2.43",
    "@times-components/webpack-configurator": "2.0.21",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "24.8.0",
    "babel-loader": "8.0.5",
    "babel-plugin-add-react-displayname": "0.0.5",
    "babel-plugin-styled-components": "1.10.0",
    "depcheck": "0.6.9",
    "enzyme": "3.9.0",
    "eslint": "5.9.0",
    "graphql": "14.2.1",
    "graphql-tag": "2.10.1",
    "jest": "24.8.0",
    "lodash.omit": "4.4.0",
    "prettier": "1.14.3",
    "react": "16.6.3",
    "react-dom": "16.6.3",
    "react-native": "0.58.6",
    "react-test-renderer": "16.6.3",
    "webpack": "4.30.0",
    "webpack-cli": "3.3.1"
  },
  "dependencies": {
    "@times-components/provider-queries": "2.2.3",
    "graphql-tag": "2.10.1",
    "hoist-non-react-statics": "2.5.5",
    "lodash.isequal": "4.4.0",
    "lodash.pick": "4.4.0",
    "prop-types": "15.7.2",
    "react-apollo": "2.5.5",
    "react-display-name": "0.2.3"
  },
  "peerDependencies": {
    "react": ">=16.5",
    "react-dom": ">=16.5",
    "react-native": ">=0.55",
    "react-native-web": "0.11.2"
  },
  "publishConfig": {
    "access": "public"
  }
}
