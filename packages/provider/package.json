{
  "name": "@times-components/provider",
  "version": "0.15.3",
  "main": "provider",
  "scripts": {
    "flow": "node_modules/flow-bin/cli.js",
    "test:watch": "jest --bail --verbose --watchAll",
    "test": "jest --bail --ci --coverage",
    "prettier:diff": "prettier --list-different '**/*.+(js|json|md)'",
    "fmt": "prettier --write '**/*.+(js|json|md)'",
    "make-schema": "node ./scripts/fetch-gql-schema",
    "lint": "eslint . && npm run prettier:diff"
  },
  "jest": {
    "preset": "react-native",
    "rootDir": "../../",
    "transformIgnorePatterns": [
      "node_modules/(?!@times-components)/"
    ],
    "testMatch": [
      "<rootDir>/packages/provider/__tests__/**.test.js"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/newsuk/times-components.git"
  },
  "keywords": [
    "react-native-web",
    "react",
    "native",
    "web",
    "provider",
    "component"
  ],
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/newsuk/times-components/issues"
  },
  "homepage": "https://github.com/newsuk/times-components#readme",
  "devDependencies": {
    "apollo-client": "1.9.3",
    "babel-cli": "6.24.1",
    "babel-core": "6.24.1",
    "babel-jest": "21.2.0",
    "babel-eslint": "7.2.3",
    "babel-loader": "7.0.0",
    "babel-plugin-transform-react-remove-prop-types": "0.4.5",
    "babel-preset-flow": "6.23.0",
    "babel-preset-react-native": "1.9.2",
    "enzyme": "3.1.0",
    "enzyme-adapter-react-16": "1.0.1",
    "eslint": "4.9.0",
    "eslint-config-airbnb": "16.1.0",
    "eslint-config-prettier": "2.6.0",
    "eslint-plugin-graphql": "1.4.0-1",
    "eslint-plugin-import": "2.8.0",
    "eslint-plugin-jsx-a11y": "6.0.2",
    "eslint-plugin-react": "7.4.0",
    "flow-bin": "0.42.0",
    "graphql": "0.11.3",
    "jest": "21.2.1",
    "mkdirp": "0.5.1",
    "nock": "9.0.14",
    "node-fetch": "1.7.2",
    "prettier": "1.8.2",
    "react": "16.0.0",
    "react-dom": "16.0.0",
    "react-native": "0.50.1",
    "react-test-renderer": "16.0.0",
    "webpack": "3.3.0"
  },
  "dependencies": {
    "@times-components/pagination": "0.10.12",
    "graphql-tag": "2.4.2",
    "lodash.get": "4.4.2",
    "lodash.pick": "4.4.0",
    "prop-types": "15.6.0",
    "react-apollo": "1.4.16",
    "react-native-web": "0.1.7"
  },
  "peerDependencies": {
    "react": ">=16",
    "react-dom": ">=16",
    "react-native": ">=0.50"
  },
  "publishConfig": {
    "access": "public"
  }
}
