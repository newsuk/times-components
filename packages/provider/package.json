{
  "name": "@times-components/provider",
  "version": "1.1.0",
  "main": "dist/provider",
  "dev": "src/provider",
  "scripts": {
    "prettier:diff": "prettier --list-different '**/*.*'",
    "fmt": "eslint . --fix && prettier --write '**/*.*'",
    "depcheck": "depcheck --ignores='@thetimes/jest-lint,babel-*,depcheck,**/eslint*,graphql,jest,prettier,webpack*' --ignore-bin-package=false --skip-missing",
    "lint": "eslint . && yarn prettier:diff && yarn depcheck && jest-lint",
    "test:web": "jest --config='./__tests__/web/jest.config.js'",
    "test": "yarn test:web && jest --config='./__tests__/jest.config.js'",
    "prepublishOnly": "yarn transpile && yarn bundle",
    "cleanup-dist": "rm -rf dist",
    "transpile": "yarn cleanup-dist && babel src -d dist",
    "bundle": "NODE_ENV=production webpack -p"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/newsuk/times-components.git"
  },
  "keywords": [
    "react-native-web",
    "react",
    "native",
    "web",
    "provider",
    "component"
  ],
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/newsuk/times-components/issues"
  },
  "homepage": "https://github.com/newsuk/times-components#readme",
  "devDependencies": {
    "@thetimes/jest-lint": "*",
    "@times-components/eslint-config-thetimes": "0.7.1",
    "@times-components/jest-configurator": "2.1.4",
    "@times-components/provider-test-tools": "1.1.0",
    "@times-components/storybook": "3.0.5",
    "@times-components/utils": "3.1.0",
    "@times-components/webpack-configurator": "2.0.4",
    "apollo-utilities": "1.0.18",
    "babel-core": "6.26.0",
    "babel-loader": "7.1.4",
    "babel-plugin-add-react-displayname": "0.0.5",
    "babel-plugin-react-native-web": "0.6.1",
    "babel-plugin-styled-components": "1.5.1",
    "babel-preset-react-native": "4.0.0",
    "depcheck": "0.6.9",
    "enzyme": "3.3.0",
    "eslint": "4.9.0",
    "graphql": "0.13.2",
    "graphql-tag": "2.8.0",
    "jest": "23.3.0",
    "prettier": "1.8.2",
    "react": "16.4.2",
    "react-dom": "16.4.2",
    "react-native": "0.55.4",
    "react-test-renderer": "16.4.2",
    "webpack": "4.6.0",
    "webpack-cli": "2.1.4"
  },
  "dependencies": {
    "@times-components/provider-queries": "1.0.2",
    "hoist-non-react-statics": "2.5.5",
    "lodash.isequal": "4.4.0",
    "lodash.pick": "4.4.0",
    "prop-types": "15.6.2",
    "react-apollo": "2.1.4",
    "react-display-name": "0.2.3"
  },
  "peerDependencies": {
    "react": ">=16.2",
    "react-dom": ">=16.2",
    "react-native": ">=0.54",
    "react-native-web": ">=0.3"
  },
  "publishConfig": {
    "access": "public"
  }
}
