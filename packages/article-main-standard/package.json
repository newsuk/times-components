{
  "name": "@times-components/article-main-standard",
  "version": "3.11.17",
  "description": "Main Standard Article Template",
  "main": "dist/article-main-standard",
  "dev": "src/article-main-standard",
  "scripts": {
    "depcheck": "depcheck --ignores='@babel/*,babel-*,depcheck,eslint,jest,prettier,webpack*' --ignore-bin-package=false --skip-missing",
    "fmt": "eslint . --fix && prettier --write '**/*.*'",
    "prettier:diff": "prettier --list-different '**/*.*'",
    "lint": "eslint . && yarn prettier:diff && yarn depcheck",
    "test:android": "jest --config='./__tests__/android/jest.config.js'",
    "test:ios": "jest --config='./__tests__/ios/jest.config.js'",
    "test:web": "jest --config='./__tests__/web/jest.config.js'",
    "test:all": "yarn test:android --coverage && yarn test:ios --coverage && yarn test:web --coverage",
    "prepublishOnly": "yarn transpile && yarn bundle",
    "watch": "watch 'yarn bundle' ./src --ignoreDotFiles --ignoreUnreadable",
    "cleanup-dist": "rm -rf dist",
    "transpile": "yarn cleanup-dist && babel src -d dist",
    "bundle": "NODE_ENV=production webpack -p"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/newsuk/times-components.git"
  },
  "keywords": [
    "react-native-web",
    "react",
    "native",
    "web",
    "article-main-standard",
    "component"
  ],
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/newsuk/times-components/issues"
  },
  "homepage": "https://github.com/newsuk/times-components#readme",
  "devDependencies": {
    "@babel/core": "7.4.4",
    "@times-components/jest-configurator": "2.6.1",
    "@times-components/jest-serializer": "3.2.13",
    "@times-components/mocks": "0.0.36",
    "@times-components/provider": "1.17.31",
    "@times-components/provider-test-tools": "1.17.34",
    "@times-components/storybook": "4.0.28",
    "@times-components/tealium-utils": "0.7.57",
    "@times-components/test-utils": "2.3.2",
    "@times-components/webpack-configurator": "2.0.23",
    "babel-jest": "24.8.0",
    "eslint": "5.9.0",
    "jest": "24.8.0",
    "jest-styled-components": "6.3.1",
    "prettier": "1.14.3",
    "react": "16.8.6",
    "react-dom": "16.8.6",
    "react-native": "0.59.10",
    "react-test-renderer": "16.6.3",
    "webpack": "4.30.0",
    "webpack-cli": "3.3.1"
  },
  "dependencies": {
    "@times-components/ad": "2.7.1",
    "@times-components/article-byline": "3.1.109",
    "@times-components/article-error": "0.2.101",
    "@times-components/article-flag": "2.6.64",
    "@times-components/article-label": "2.4.6",
    "@times-components/article-lead-asset": "1.3.26",
    "@times-components/article-skeleton": "1.12.2",
    "@times-components/article-topics": "4.0.44",
    "@times-components/caption": "3.0.59",
    "@times-components/context": "1.0.35",
    "@times-components/date-publication": "0.20.87",
    "@times-components/responsive": "0.4.59",
    "@times-components/styleguide": "3.28.41",
    "@times-components/user-state": "0.0.20",
    "@times-components/utils": "4.11.28",
