    "@times-components-native/article-label": "2.7.47",
    "@times-components-native/article-lead-asset": "1.6.24",
    "@times-components-native/article-skeleton": "1.34.1",
    "@times-components-native/article-topics": "4.1.36",
    "@times-components-native/caption": "3.0.131",
