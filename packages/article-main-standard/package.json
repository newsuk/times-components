    "prop-types": "15.7.2",
    "styled-components": "4.2.0"
  },
  "peerDependencies": {
    "react": ">=16.5",
    "react-dom": ">=16.5",
    "react-native": ">=0.55",
    "react-native-web": "0.11.4"
  },
  "publishConfig": {
    "access": "public"
  }
}
