{
  "name": "@times-components/utils",
  "version": "6.12.0",
  "description": "A set of helpers and/or workarounds to be shared across packages",
  "main": "dist/index",
  "dev": "src/index",
  "scripts": {
    "prettier:diff": "prettier --list-different '**/*.*'",
    "fmt": "eslint . --fix && prettier --write '**/*.*'",
    "depcheck": "depcheck --ignores='@babel/*,babel-*,depcheck,eslint,jest,prettier,webpack*' --ignore-bin-package=false --skip-missing",
    "lint": "eslint . && yarn prettier:diff && yarn depcheck",
    "test": "jest --config='./__tests__/jest.config.js'",
    "prepublishOnly": "yarn transpile && yarn bundle",
    "transpile": "babel src -d dist",
    "watch": "watch 'yarn bundle' ./src --ignoreDotFiles --ignoreUnreadable",
    "bundle": "NODE_ENV=production webpack -p"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/newsuk/times-components.git"
  },
  "keywords": [
    "utils",
    "graphql"
  ],
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/newsuk/times-components/issues"
  },
  "homepage": "https://github.com/newsuk/times-components#readme",
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@babel/core": "7.4.4",
    "@times-components/eslint-config-thetimes": "0.8.18",
    "@times-components/jest-configurator": "2.8.0",
    "@times-components/jest-serializer": "3.2.29",
    "@times-components/webpack-configurator": "2.0.29",
    "babel-jest": "24.8.0",
    "babel-loader": "8.0.5",
    "babel-plugin-add-react-displayname": "0.0.5",
    "babel-plugin-styled-components": "1.10.6",
    "depcheck": "0.6.9",
    "eslint": "5.9.0",
    "jest": "24.8.0",
    "prettier": "1.14.3",
    "react-test-renderer": "16.9.0",
    "webpack": "4.30.0",
    "webpack-cli": "3.3.1"
  },
  "dependencies": {
    "@times-components/schema": "0.7.1",
    "@times-components/ts-styleguide": "1.36.1",
    "apollo-cache-inmemory": "1.5.1",
    "apollo-client": "2.5.1",
    "apollo-link": "1.2.4",
    "apollo-link-http": "1.5.14",
    "apollo-link-persisted-queries": "0.2.2",
    "lodash.omitby": "4.6.0",
    "prop-types": "15.7.2",
    "styled-components": "4.3.2",
    "unfetch": "^3.0.0"
  },
  "resolutions": {
    "react": "16.9.0",
    "react-dom": "16.9.0"
  },
  "peerDependencies": {
    "react": ">=16.9",
    "react-dom": ">=16.9"
  }
}
