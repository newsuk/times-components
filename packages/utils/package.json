{
  "name": "@times-components/utils",
  "version": "0.6.7",
  "description": "A set of helpers and/or workarounds to be shared across packages",
  "main": "dist",
  "dev": "src",
  "scripts": {
    "make-schema": "node ./scripts/fetch-gql-schema",
    "prettier:diff": "prettier --list-different '**/*.*'",
    "fmt": "prettier --write '**/*.*'",
    "lint": "eslint . && yarn prettier:diff",
    "test": "jest --config='./src/__tests__/jest.config.js'",
    "prepublishOnly": "yarn transpile",
    "transpile": "babel src --ignore node_modules,__tests__ -d dist && cp src/schema.json dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/newsuk/times-components.git"
  },
  "keywords": [
    "utils",
    "graphql"
  ],
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/newsuk/times-components/issues"
  },
  "homepage": "https://github.com/newsuk/times-components#readme",
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@times-components/eslint-config-thetimes": "0.4.0",
    "@times-components/jest-configurator": "0.5.0",
    "babel-plugin-add-react-displayname": "0.0.5",
    "babel-plugin-flow-react-proptypes": "22.0.0",
    "babel-plugin-styled-components": "1.5.1",
    "babel-preset-react-native": "4.0.0",
    "eslint": "4.9.0",
    "jest": "21.2.1",
    "mkdirp": "0.5.1",
    "node-fetch": "1.7.2",
    "prettier": "1.8.2"
  },
  "dependencies": {
    "apollo-cache-inmemory": "1.1.5",
    "apollo-client": "2.2.3",
    "graphql": "0.12.3",
    "prop-types": "15.6.0",
    "react": "16.2.0",
    "react-apollo": "2.1.0-rc.3"
  },
  "peerDependencies": {
    "react": ">=16",
    "react-dom": ">=16",
    "react-native": ">=0.50",
    "react-native-web": ">=0.3"
  }
}
