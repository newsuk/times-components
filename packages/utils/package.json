{
  "name": "@times-components/utils",
  "version": "2.2.1",
  "description": "A set of helpers and/or workarounds to be shared across packages",
  "main": "dist",
  "dev": "src",
  "scripts": {
    "make-schema": "node ./scripts/fetch-gql-schema",
    "prettier:diff": "prettier --list-different '**/*.*'",
    "fmt": "eslint . --fix && prettier --write '**/*.*'",
    "depcheck": "depcheck --ignores='@thetimes/jest-lint,babel-*,depcheck,eslint,jest,prettier,webpack*' --ignore-bin-package=false --skip-missing",
    "lint": "eslint . && yarn prettier:diff && yarn depcheck && jest-lint",
    "test": "jest --config='./__tests__/jest.config.js'",
    "prepublishOnly": "yarn transpile && yarn bundle",
    "transpile": "babel src -d dist && cp src/schema.json dist",
    "bundle": "NODE_ENV=production webpack -p"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/newsuk/times-components.git"
  },
  "keywords": [
    "utils",
    "graphql"
  ],
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/newsuk/times-components/issues"
  },
  "homepage": "https://github.com/newsuk/times-components#readme",
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@thetimes/jest-lint": "*",
    "@times-components/eslint-config-thetimes": "0.7.0",
    "@times-components/jest-configurator": "2.1.1",
    "@times-components/webpack-configurator": "2.0.2",
    "babel-core": "6.26.0",
    "babel-loader": "7.1.4",
    "babel-plugin-add-react-displayname": "0.0.5",
    "babel-plugin-flow-react-proptypes": "22.0.0",
    "babel-plugin-react-native-web": "0.6.1",
    "babel-plugin-styled-components": "1.5.1",
    "babel-preset-react-native": "4.0.0",
    "depcheck": "0.6.9",
    "eslint": "4.9.0",
    "jest": "23.3.0",
    "mkdirp": "0.5.1",
    "node-fetch": "1.7.2",
    "prettier": "1.8.2",
    "webpack": "4.6.0",
    "webpack-cli": "2.1.4"
  },
  "dependencies": {
    "apollo-cache-inmemory": "1.1.10",
    "graphql": "0.13.2",
    "lodash.omitby": "4.6.0",
    "react-native": "0.55.4"
  },
  "peerDependencies": {
    "react": ">=16.2",
    "react-dom": ">=16.2",
    "react-native": ">=0.54",
    "react-native-web": ">=0.3"
  }
}
