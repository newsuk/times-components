{
  "name": "@times-components/tealium-utils",
  "version": "0.7.8",
  "description": "React tealium helpers for Times Components",
  "main": "dist",
  "dev": "src",
  "scripts": {
    "fmt": "eslint . --fix && prettier --write '**/*.*'",
    "prettier:diff": "prettier --list-different '**/*.*'",
    "depcheck": "depcheck --ignores='@thetimes/jest-lint,babel-*,depcheck,eslint,jest,prettier,webpack*' --ignore-bin-package=false --skip-missing",
    "lint": "eslint . && yarn prettier:diff && yarn depcheck",
    "prepublishOnly": "yarn transpile && yarn bundle",
    "cleanup-dist": "rm -rf dist",
    "transpile": "yarn cleanup-dist && babel src --ignore node_modules -d dist",
    "bundle": "NODE_ENV=production webpack -p"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/newsuk/times-components.git"
  },
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/newsuk/times-components/issues"
  },
  "homepage": "https://github.com/newsuk/times-components#readme",
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.0",
    "@times-components/eslint-config-thetimes": "0.8.8",
    "babel-loader": "8.0.4",
    "depcheck": "0.6.9",
    "eslint": "5.9.0",
    "metro-react-native-babel-preset": "0.49.2",
    "prettier": "1.14.3",
    "webpack": "4.22.0",
    "webpack-cli": "3.1.2"
  },
  "dependencies": {
    "@storybook/addon-actions": "^4.0.0-alpha.25",
    "@times-components/tealium": "1.6.12"
  },
  "peerDependencies": {
    "react": ">=16.5",
    "react-dom": ">=16.5",
    "react-native": ">=0.55",
    "react-native-web": "0.9.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
