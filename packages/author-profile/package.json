    "@times-components/tealium-utils": "0.7.76",
    "@times-components/test-utils": "2.3.11",
    "@times-components/webpack-configurator": "2.0.30",
    "apollo-client": "2.5.1",
    "babel-jest": "24.8.0",
    "babel-loader": "8.0.5",
    "depcheck": "0.6.9",
    "eslint": "5.9.0",
    "jest": "24.8.0",
    "jest-styled-components": "6.3.1",
    "mockdate": "3.0.5",
    "prettier": "1.14.3",
    "react": "16.9.0",
