    "babel-preset-flow": "6.23.0",
    "babel-preset-react-native": "1.9.2",
    "chance": "1.0.10",
    "enzyme": "3.1.0",
    "enzyme-adapter-react-16": "1.0.1",
    "enzyme-to-json": "3.0.1",
    "eslint": "4.9.0",
