    "eslint-config-prettier": "2.6.0",
    "eslint-plugin-import": "2.8.0",
    "eslint-plugin-jsx-a11y": "6.0.2",
    "eslint-plugin-react": "7.4.0",
    "flow-bin": "0.42.0",
    "jest": "21.2.1",
    "jsdom": "11.1.0",
    "prettier": "1.8.2",
    "react": "16.0.0",
    "react-dom": "16.0.0",
    "react-native": "0.50.1",
    "react-test-renderer": "16.0.0",
    "webpack": "3.3.0"
