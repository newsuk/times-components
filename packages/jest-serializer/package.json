{
  "name": "@times-components/jest-serializer",
  "version": "3.0.1",
  "description": "serializer",
  "main": "dist/index",
  "dev": "src/jest-serializer",
  "scripts": {
    "fmt": "eslint . --fix && prettier --write '**/*.*'",
    "prettier:diff": "prettier --list-different '**/*.*'",
    "depcheck": "depcheck --ignores='babel-*,depcheck,eslint,identity-obj-proxy,jest,prettier,webpack*' --ignore-bin-package=false --skip-missing",
    "lint": "eslint . && yarn prettier:diff && yarn depcheck",
    "test:ios": "jest --config='./jest.config.native.js'",
    "test:web": "jest --config='./jest.config.web.js'",
    "prepublishOnly": "yarn transpile",
    "cleanup-dist": "rm -rf dist",
    "transpile": "yarn cleanup-dist && babel src -d dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/newsuk/times-components.git"
  },
  "keywords": [
    "react-native-web",
    "react",
    "native",
    "web",
    "jest-serializer",
    "component"
  ],
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/newsuk/times-components/issues"
  },
  "homepage": "https://github.com/newsuk/times-components#readme",
  "devDependencies": {
    "@times-components/eslint-config-thetimes": "0.7.1",
    "depcheck": "0.6.9",
    "enzyme": "3.3.0",
    "enzyme-adapter-react-16": "1.1.0",
    "eslint": "4.9.0",
    "identity-obj-proxy": "3.0.0",
    "jest": "23.3.0",
    "prettier": "1.8.2",
    "prop-types": "15.6.2",
    "react-native-web": "0.5.1",
    "react-test-renderer": "16.4.2"
  },
  "dependencies": {
    "enzyme-to-json": "3.3.4",
    "lodash.omitby": "4.6.0"
  },
  "peerDependencies": {
    "react": ">=16",
    "react-native": ">=0.53.0",
    "react-native-web": ">=0.3"
  },
  "publishConfig": {
    "access": "public"
  }
}
