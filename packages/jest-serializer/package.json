{
  "name": "@times-components/jest-serializer",
  "version": "3.6.0",
  "description": "serializer",
  "main": "dist/index",
  "dev": "src/jest-serializer",
  "scripts": {
    "fmt": "eslint . --fix && prettier --write '**/*.*'",
    "prettier:diff": "prettier --list-different '**/*.*'",
    "depcheck": "depcheck --ignores='@babel/*,babel-*,depcheck,eslint,identity-obj-proxy,jest,prettier,react-art,webpack*' --ignore-bin-package=false --skip-missing",
    "lint": "eslint . && yarn prettier:diff && yarn depcheck",
    "test:web": "jest --config='./__tests__/jest.config.js'",
    "test:web:updatesnapshot": "yarn test:web -u",
    "prepublishOnly": "yarn transpile",
    "cleanup-dist": "rm -rf dist",
    "transpile": "yarn cleanup-dist && babel src -d dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/newsuk/times-components.git"
  },
  "keywords": [
    "react",
    "jest-serializer",
    "component"
  ],
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/newsuk/times-components/issues"
  },
  "homepage": "https://github.com/newsuk/times-components#readme",
  "devDependencies": {
    "@babel/core": "7.4.4",
    "@times-components/eslint-config-thetimes": "0.8.18",
    "@times-components/jest-configurator-web": "0.8.0",
    "babel-jest": "24.8.0",
    "depcheck": "0.6.9",
    "enzyme": "3.9.0",
    "enzyme-adapter-react-16": "1.12.1",
    "eslint": "5.9.0",
    "jest": "24.8.0",
    "prettier": "1.14.3",
    "prop-types": "15.7.2",
    "react-test-renderer": "16.9.0"
  },
  "dependencies": {
    "@times-components/utils": "6.15.16",
    "css": "^2.2.1",
    "enzyme-to-json": "3.3.5",
    "hyphenate-style-name": "1.0.2",
    "lodash.isequal": "4.4.0",
    "lodash.omitby": "4.6.0"
  },
  "resolutions": {
    "react": "16.9.0",
    "react-dom": "16.9.0"
  },
  "peerDependencies": {
    "react": ">=16",
    "react-dom": ">=16"
  },
  "publishConfig": {
    "access": "public"
  }
}
