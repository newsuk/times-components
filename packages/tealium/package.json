{
  "name": "@times-components/tealium",
  "version": "1.6.12",
  "description": "React component for tealium reporter",
  "main": "dist",
  "dev": "src",
  "scripts": {
    "fmt": "eslint . --fix && prettier --write '**/*.*'",
    "prettier:diff": "prettier --list-different '**/*.*'",
    "depcheck": "depcheck --ignores='@thetimes/jest-lint,babel-*,depcheck,eslint,jest,prettier,react-art,webpack*' --ignore-bin-package=false --skip-missing",
    "lint": "eslint . && yarn prettier:diff && yarn depcheck && jest-lint",
    "test:web": "jest --config='./__tests__/web/jest.config.js'",
    "prepublishOnly": "yarn transpile && yarn bundle",
    "bundle": "NODE_ENV=production webpack -p",
    "cleanup-dist": "rm -rf dist",
    "transpile": "yarn cleanup-dist && babel src -d dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/newsuk/times-components.git"
  },
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/newsuk/times-components/issues"
  },
  "homepage": "https://github.com/newsuk/times-components#readme",
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.0",
    "@babel/plugin-transform-react-display-name": "7.0.0",
    "@thetimes/jest-lint": "*",
    "@times-components/eslint-config-thetimes": "0.8.8",
    "@times-components/jest-configurator": "2.1.17",
    "@times-components/test-utils": "2.0.3",
    
    "babel-jest": "^23.4.2",
    "babel-loader": "8.0.4",
    "depcheck": "0.6.9",
    "eslint": "5.9.0",
    "jest": "23.6.0",
    "metro-react-native-babel-preset": "0.49.2",
    "prettier": "1.14.3",
    "react-art": "16.5.2",
    "react-native-web": "0.9.0",
    "webpack": "4.22.0",
    "webpack-cli": "3.1.2"
  },
  "peerDependencies": {
    "react": ">=16.5",
    "react-dom": ">=16.5",
    "react-native": ">=0.55",
    "react-native-web": "0.9.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
