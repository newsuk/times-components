{
  "name": "@times-components/image",
  "version": "4.3.16",
  "main": "dist/index",
  "dev": "src/index",
  "scripts": {
    "fmt": "eslint . --fix && prettier --write '**/*.*'",
    "prettier:diff": "prettier --list-different '**/*.*'",
    "depcheck": "depcheck --ignores='@thetimes/jest-lint,babel-*,depcheck,eslint,jest,prettier,react-art,stylelint,webpack*' --ignore-bin-package=false --skip-missing",
    "lint": "eslint . && stylelint . && yarn prettier:diff && yarn depcheck && jest-lint",
    "test:android": "jest --config='./__tests__/android/jest.config.js'",
    "test:android:debug": "node --inspect --inspect-brk node_modules/.bin/jest --config='./__tests__/android/jest.config.js'",
    "test:ios": "jest --config='./__tests__/ios/jest.config.js'",
    "test:web": "jest --config='./__tests__/web/jest.config.js'",
    "test:all": "yarn test:android --coverage && yarn test:ios --coverage && yarn test:web --coverage",
    "prepublishOnly": "yarn transpile && yarn bundle",
    "cleanup-dist": "rm -rf dist",
    "transpile": "yarn cleanup-dist && babel src -d dist",
    "bundle": "NODE_ENV=production webpack -p"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/newsuk/times-components.git"
  },
  "keywords": [
    "react-native-web",
    "react",
    "native",
    "web",
    "image",
    "component"
  ],
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/newsuk/times-components/issues"
  },
  "homepage": "https://github.com/newsuk/times-components#readme",
  "devDependencies": {
    "@thetimes/jest-lint": "*",
    "@times-components/eslint-config-thetimes": "0.8.8",
    "@times-components/jest-configurator": "2.1.17",
    "@times-components/jest-serializer": "3.1.10",
    "@times-components/storybook": "3.2.0",
    "@times-components/test-utils": "2.0.3",
    "@times-components/webpack-configurator": "2.0.14",
    "babel-core": "6.26.0",
    "babel-loader": "7.1.4",
    "babel-plugin-add-react-displayname": "0.0.5",
    "babel-plugin-styled-components": "1.5.1",
    "babel-preset-react-native": "4.0.0",
    "depcheck": "0.6.9",
    "eslint": "5.9.0",
    "jest": "23.3.0",
    "jest-styled-components": "5.0.1",
    "prettier": "1.14.3",
    "react": "16.5.2",
    "react-art": "16.5.2",
    "react-dom": "16.5.2",
    "react-native": "0.55.4",
    "react-native-web": "0.9.0",
    "react-test-renderer": "16.5.2",
    "stylelint": "9.4.0",
    "webpack": "4.6.0",
    "webpack-cli": "2.1.4"
  },
  "dependencies": {
    "@times-components/caption": "2.2.4",
    "@times-components/gestures": "3.2.10",
    "@times-components/gradient": "2.2.17",
    "@times-components/link": "3.1.15",
    "@times-components/styleguide": "3.9.1",
    "@times-components/svgs": "2.1.21",
    "@times-components/utils": "4.0.14",
    "prop-types": "15.6.2",
    "styled-components": "3.4.0"
  },
  "peerDependencies": {
    "react": ">=16.5",
    "react-dom": ">=16.5",
    "react-native": ">=0.55",
    "react-native-web": "0.9.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
