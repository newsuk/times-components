{
  "name": "@times-components/image",
  "version": "1.6.0",
  "main": "image",
  "scripts": {
    "flow": "node_modules/flow-bin/cli.js",
    "test:watch": "jest --bail --verbose --watchAll",
    "test": "jest --bail --ci --coverage",
    "fmt": "prettier --write '**/*.+(js|json)' '**/*.md' '!**/CHANGELOG.md'",
    "prettier:diff": "prettier --list-different '**/*.+(js|json)' '**/*.md' '!**/CHANGELOG.md'",
    "lint": "eslint . && npm run prettier:diff"
  },
  "jest": {
    "preset": "react-native",
    "rootDir": "../../",
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!@times-components)/"
    ],
    "testMatch": [
      "<rootDir>/packages/image/**.test.js"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/newsuk/times-components.git"
  },
  "keywords": [
    "react-native-web",
    "react",
    "native",
    "web",
    "image",
    "component"
  ],
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/newsuk/times-components/issues"
  },
  "homepage": "https://github.com/newsuk/times-components#readme",
  "devDependencies": {
    "babel-cli": "6.24.1",
    "babel-core": "6.24.1",
    "babel-loader": "7.0.0",
    "babel-plugin-transform-react-remove-prop-types": "0.4.5",
    "babel-preset-flow": "6.23.0",
    "babel-preset-react-native": "1.9.2",
    "enzyme": "3.1.0",
    "enzyme-adapter-react-16": "1.0.1",
    "enzyme-to-json": "3.0.1",
    "eslint": "4.9.0",
    "eslint-config-airbnb": "16.1.0",
    "eslint-config-prettier": "2.6.0",
    "eslint-plugin-import": "2.8.0",
    "eslint-plugin-jsx-a11y": "6.0.2",
    "eslint-plugin-react": "7.4.0",
    "flow-bin": "0.42.0",
    "jest": "21.2.1",
    "prettier": "1.8.2",
    "react": "16.0.0",
    "react-dom": "16.0.0",
    "react-native": "0.49.3",
    "react-native-linear-gradient": "2.3.0",
    "react-test-renderer": "16.0.0",
    "webpack": "3.3.0"
  },
  "dependencies": {
    "@times-components/gradient": "0.1.0",
    "prop-types": "15.6.0",
    "react-native-web": "0.1.7",
    "react-style-proptype": "3.0.0",
    "svgs": "3.1.0"
  },
  "peerDependencies": {
    "react": ">=16",
    "react-dom": ">=16",
    "react-native": ">=0.49",
    "react-native-linear-gradient": ">=2.3"
  },
  "publishConfig": {
    "access": "public"
  }
}
