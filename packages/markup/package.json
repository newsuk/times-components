{
  "name": "@times-components/markup",
  "version": "0.17.0",
  "description": "a component which takes an HTML ast and renders the platform specific markup",
  "main": "markup",
  "scripts": {
    "test:watch": "yarn test:native --bail --verbose --watchAll",
    "test:native": "jest --config='./jest.config.json'",
    "test:web": "jest --config='./jest.config.web.json'",
    "test": "yarn test:native --ci --bail --coverage && yarn test:web --ci --bail --coverage",
    "fmt": "prettier --write '**/*.*'",
    "prettier:diff": "prettier --list-different '**/*.*'",
    "lint": "eslint . && yarn prettier:diff",
    "dextrose-stories": "dextrose-stories",
    "dextrose-clean": "dextrose-clean"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/newsuk/times-components.git"
  },
  "keywords": [
    "react-native-web",
    "react",
    "native",
    "web",
    "markup",
    "component"
  ],
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/newsuk/times-components/issues"
  },
  "homepage": "https://github.com/newsuk/times-components#readme",
  "devDependencies": {
    "@storybook/react-native": "3.3.11",
    "@times-components/eslint-config-thetimes": "0.1.3",
    "@times-components/storybook": "0.2.6",
    "dextrose": "1.4.2",
    "eslint": "4.9.0",
    "jest": "21.2.1",
    "prettier": "1.8.2",
    "react": "16.2.0",
    "react-dom": "16.2.0",
    "react-native": "0.50.1",
    "react-native-web": "0.3.2",
    "react-test-renderer": "16.2.0"
  },
  "dependencies": {
    "@times-components/ad": "0.7.0",
    "@times-components/pull-quote": "0.2.7",
    "prop-types": "15.6.0"
  },
  "peerDependencies": {
    "babel-preset-env": "1.6.1",
    "babel-preset-stage-2": "6.24.1",
    "react": ">=16",
    "react-dom": ">=16",
    "react-native": ">=0.50",
    "react-native-web": ">=0.3"
  },
  "publishConfig": {
    "access": "public"
  }
}
