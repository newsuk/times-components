{
  "preset": "react-native",
  "rootDir": "../../",
  "coverageDirectory": "<rootDir>/packages/markup/coverage/native/",
  "collectCoverageFrom": [
    "**/packages/markup/*.js",
    "!**/packages/markup/*.web.js",
    "!**/packages/markup/__tests__/**",
    "!**/packages/markup/*.stories.js"
  ],
  "transformIgnorePatterns": [
    "node_modules/(?!(react-native|@times-components)/)"
  ],
  "testMatch": ["<rootDir>/packages/markup/__tests__/markup.native.test.js"]
}
