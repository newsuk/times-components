{
  "name": "@times-components/article-in-depth",
  "version": "3.9.1",
  "description": "In Depth Article Template",
  "main": "dist/article-in-depth",
  "dev": "src/article-in-depth",
  "scripts": {
    "depcheck": "depcheck --ignores='babel-*,depcheck,eslint,jest,prettier,webpack*' --ignore-bin-package=false --skip-missing",
    "fmt": "eslint . --fix && prettier --write '**/*.*'",
    "prettier:diff": "prettier --list-different '**/*.*'",
    "lint": "eslint . && yarn prettier:diff && yarn depcheck",
    "test:android": "jest --config='./__tests__/android/jest.config.js'",
    "test:ios": "jest --config='./__tests__/ios/jest.config.js'",
    "test:web": "jest --config='./__tests__/web/jest.config.js'",
    "test:all": "yarn test:android --coverage && yarn test:ios --coverage && yarn test:web --coverage",
    "prepublishOnly": "yarn transpile && yarn bundle",
    "watch": "watch 'yarn bundle' ./src --ignoreDotFiles --ignoreUnreadable",
    "cleanup-dist": "rm -rf dist",
    "transpile": "yarn cleanup-dist && babel src -d dist",
    "bundle": "NODE_ENV=production webpack -p"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/newsuk/times-components.git"
  },
  "keywords": [
    "react-native-web",
    "react",
    "native",
    "web",
    "article-in-depth",
    "component"
  ],
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/newsuk/times-components/issues"
  },
  "homepage": "https://github.com/newsuk/times-components#readme",
  "devDependencies": {
    "@times-components/eslint-config-thetimes": "0.8.12",
    "@times-components/jest-configurator": "2.5.14",
    "@times-components/jest-serializer": "3.2.10",
    "@times-components/mocks": "0.0.31",
    "@times-components/provider": "1.17.25",
    "@times-components/provider-test-tools": "1.17.29",
    "@times-components/storybook": "4.0.23",
    "@times-components/tealium-utils": "0.7.54",
    "@times-components/test-utils": "2.3.0",
    "@times-components/webpack-configurator": "2.0.22",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "24.8.0",
    "eslint": "5.9.0",
    "jest": "24.8.0",
    "jest-styled-components": "6.3.1",
    "prettier": "1.14.3",
    "react": "16.8.6",
    "react-dom": "16.8.6",
    "react-native": "0.59.10",
    "react-test-renderer": "16.6.3",
    "webpack": "4.30.0",
    "webpack-cli": "3.3.1"
  },
  "dependencies": {
    "@times-components/ad": "2.6.32",
    "@times-components/article-byline": "3.1.100",
    "@times-components/article-error": "0.2.96",
    "@times-components/article-flag": "2.6.58",
    "@times-components/article-label": "2.4.1",
    "@times-components/article-lead-asset": "1.3.16",
    "@times-components/article-skeleton": "1.11.8",
    "@times-components/caption": "3.0.51",
    "@times-components/context": "1.0.30",
    "@times-components/date-publication": "0.20.81",
    "@times-components/responsive": "0.4.54",
    "@times-components/styleguide": "3.28.36",
    "@times-components/user-state": "0.0.15",
    "@times-components/utils": "4.11.23",
    "@times-components/video-label": "2.3.0",
    "prop-types": "15.7.2",
    "styled-components": "4.2.0"
  },
  "peerDependencies": {
    "react": ">=16.5",
    "react-dom": ">=16.5",
    "react-native": ">=0.55",
    "react-native-web": "0.11.4"
  },
  "publishConfig": {
    "access": "public"
  }
