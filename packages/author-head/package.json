{
  "name": "@times-components/author-head",
  "version": "0.25.9",
  "main": "dist/author-head",
  "dev": "src/author-head",
  "scripts": {
    "fmt": "prettier --write '**/*.*'",
    "prettier:diff": "prettier --list-different '**/*.*'",
    "lint": "eslint . && yarn prettier:diff",
    "test:android": "jest --config='./__tests__/android/jest.config.js'",
    "test:ios": "jest --config='./__tests__/ios/jest.config.js'",
    "test:web": "jest --config='./__tests__/web/jest.config.js'",
    "test:all": "yarn test:android --coverage & yarn test:ios --coverage & yarn test:web --coverage",
    "dextrose-stories": "dextrose generate-stories $(pwd)",
    "dextrose-clean": "dextrose clean-stories $(pwd)",
    "prepublishOnly": "yarn transpile",
    "cleanup-dist": "rm -rf dist",
    "transpile": "yarn cleanup-dist && babel src -d dist"
  },
  "jest": {
    "preset": "react-native",
    "rootDir": "../../",
    "coverageDirectory": "<rootDir>/packages/author-head/coverage/",
    "transformIgnorePatterns": [
      "node_modules/(?!(react-native|@times-components)/)"
    ],
    "testMatch": [
      "<rootDir>/packages/author-head/__tests__/*.test.js"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/newsuk/times-components.git"
  },
  "keywords": [
    "react-native-web",
    "react",
    "native",
    "web",
    "author-head",
    "component"
  ],
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/newsuk/times-components/issues"
  },
  "homepage": "https://github.com/newsuk/times-components#readme",
  "devDependencies": {
    "@times-components/eslint-config-thetimes": "0.4.1",
    "@times-components/jest-configurator": "0.6.2",
    "@times-components/storybook": "0.9.4",
    "@times-components/tealium-utils": "0.2.5",
    "babel-core": "6.26.0",
    "babel-plugin-add-react-displayname": "0.0.5",
    "babel-plugin-flow-react-proptypes": "22.0.0",
    "babel-plugin-styled-components": "1.5.1",
    "babel-preset-react-native": "4.0.0",
    "dextrose": "1.7.6",
    "enzyme": "3.3.0",
    "enzyme-to-json": "3.3.0",
    "eslint": "4.9.0",
    "jest": "21.2.1",
    "jest-styled-components": "5.0.0",
    "prettier": "1.8.2",
    "react": "16.3.1",
    "react-dom": "16.3.1",
    "react-native": "0.54.2",
    "react-native-web": "0.3.2",
    "react-test-renderer": "16.3.1"
  },
  "dependencies": {
    "@times-components/gradient": "0.10.7",
    "@times-components/icons": "0.7.7",
    "@times-components/image": "1.17.5",
    "@times-components/link": "0.20.7",
    "@times-components/markup": "0.26.10",
    "@times-components/responsive-styles": "0.7.3",
    "@times-components/styleguide": "0.6.5",
    "@times-components/tracking": "0.12.10",
    "prop-types": "15.6.0",
    "styled-components": "3.2.2"
  },
  "peerDependencies": {
    "react": ">=16.2",
    "react-dom": ">=16.2",
    "react-native": ">=0.54",
    "react-native-web": ">=0.3"
  },
  "publishConfig": {
    "access": "public"
  }
}
