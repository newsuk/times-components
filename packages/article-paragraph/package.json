{
  "name": "@times-components/article-paragraph",
  "version": "1.3.47",
  "description": "Article Paragraph",
  "main": "dist/",
  "dev": "src/",
  "scripts": {
    "depcheck": "depcheck --ignores='@babel/*,babel-*,depcheck,eslint,jest,prettier,webpack*' --ignore-bin-package=false --skip-missing",
    "fmt": "eslint . --fix && prettier --write '**/*.*'",
    "prettier:diff": "prettier --list-different '**/*.*'",
    "lint": "eslint . && yarn prettier:diff && yarn depcheck",
    "test:android": "jest --config='./__tests__/android/jest.config.js'",
    "test:ios": "jest --config='./__tests__/ios/jest.config.js'",
    "test:web": "jest --config='./__tests__/web/jest.config.js'",
    "test:all": "yarn test:android --coverage && yarn test:ios --coverage && yarn test:web --coverage",
    "prepublishOnly": "yarn transpile && yarn bundle",
    "watch": "watch 'yarn bundle' ./src --ignoreDotFiles --ignoreUnreadable",
    "cleanup-dist": "rm -rf dist",
    "transpile": "yarn cleanup-dist && babel src -d dist",
    "bundle": "NODE_ENV=production webpack -p"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/newsuk/times-components.git"
  },
  "keywords": [
    "react-native-web",
    "react",
    "native",
    "web",
    "article-paragraph",
    "component"
  ],
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/newsuk/times-components/issues"
  },
  "homepage": "https://github.com/newsuk/times-components#readme",
  "dependencies": {
    "@times-components/article-skeleton": "1.13.5",
    "@times-components/context": "1.0.36",
    "@times-components/markup-forest": "1.7.20",
    "@times-components/responsive": "0.4.60",
    "@times-components/styleguide": "3.28.42",
    "prop-types": "15.7.2",
    "styled-components": "4.2.0"
  },
  "devDependencies": {
    "@babel/core": "7.4.4",
    "@times-components/eslint-config-thetimes": "0.8.13",
    "@times-components/jest-configurator": "2.6.2",
    "@times-components/jest-serializer": "3.2.14",
    "@times-components/markup": "3.3.98",
    "@times-components/mocks": "0.0.37",
    "@times-components/storybook": "4.0.29",
    "@times-components/test-utils": "2.3.3",
    "@times-components/text-flow": "0.2.8",
    "@times-components/webpack-configurator": "2.0.24",
    "babel-jest": "24.8.0",
    "babel-plugin-styled-components": "1.10.0",
    "eslint": "5.9.0",
    "jest": "24.8.0",
    "jest-styled-components": "6.3.1",
    "prettier": "1.14.3",
    "react": "16.8.6",
    "react-dom": "16.8.6",
    "react-native": "0.59.10",
    "react-test-renderer": "16.6.3",
    "webpack": "4.30.0",
    "webpack-cli": "3.3.1"
  },
  "peerDependencies": {
    "react": ">=16.5",
    "react-dom": ">=16.5",
    "react-native": ">=0.55",
    "react-native-web": "0.11.4"
  },
  "publishConfig": {
    "access": "public"
  }
}
