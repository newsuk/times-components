{
  "name": "@times-components/related-articles",
  "version": "0.0.1",
  "description": "Related articles",
  "main": "related-articles",
  "scripts": {
    "fmt": "prettier --write '**/*.*'",
    "prettier:diff": "prettier --list-different '**/*.*'",
    "lint": "eslint . && npm run prettier:diff",
    "test": "yarn test:android -- --ci --bail --coverage && yarn test:ios -- --ci --bail --coverage && yarn test:web -- --ci --bail --coverage",
    "test:android": "jest --config='./__tests__/android/jest.config.js'",
    "test:ios": "jest --config='./__tests__/ios/jest.config.js'",
    "test:web": "jest --config='./__tests__/web/jest.config.js'",
    "test:watch": "jest --projects */**/jest.config.js --watchAll"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/newsuk/times-components.git"
  },
  "keywords": [
    "react-native-web",
    "react",
    "native",
    "web",
    "related-articles",
    "component"
  ],
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/newsuk/times-components/issues"
  },
  "homepage": "https://github.com/newsuk/times-components#readme",
  "devDependencies": {
    "@storybook/addon-actions": "3.2.18",
    "@storybook/react-native": "3.2.11",
    "@times-components/jest-configurator": "0.0.8",
    "eslint": "4.9.0",
    "jest": "21.2.1",
    "prettier": "1.8.2",
    "dextrose": "1.2.14",
    "react": "16.2.0",
    "react-dom": "16.2.0",
    "react-native": "0.50.1",
    "react-test-renderer": "16.2.0"
  },
  "dependencies": {
    "@times-components/article-summary": "0.14.2",
    "@times-components/card": "0.16.23",
    "@times-components/image": "1.12.3",
    "@times-components/link": "0.13.7",
    "@times-components/markup": "0.15.11",
    "@times-components/responsive-styles": "0.2.9",
    "lodash.get": "4.4.2",
    "prop-types": "15.6.0"
  },
  "peerDependencies": {
    "react": ">=16",
    "react-dom": ">=16",
    "react-native": ">=0.50",
    "react-native-web": ">=0.1"
  },
  "publishConfig": {
    "access": "public"
  }
