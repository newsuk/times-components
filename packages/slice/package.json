{
  "name": "@times-components/slice",
  "version": "0.17.12",
  "description": "Slice",
  "main": "dist/slice",
  "scripts": {
    "fmt": "prettier --write '**/*.*'",
    "prettier:diff": "prettier --list-different '**/*.*'",
    "lint": "eslint . && npm run prettier:diff",
    "test:android": "jest --config='./__tests__/android/jest.config.js'",
    "test:ios": "jest --config='./__tests__/ios/jest.config.js'",
    "test:web": "jest --config='./__tests__/web/jest.config.js'",
    "test:all": "yarn test:android --coverage & yarn test:ios --coverage & yarn test:web --coverage",
    "prepublishOnly": "yarn transpile",
    "transpile": "babel src -d dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/newsuk/times-components.git"
  },
  "keywords": [
    "react-native-web",
    "react",
    "native",
    "web",
    "slice",
    "component"
  ],
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/newsuk/times-components/issues"
  },
  "homepage": "https://github.com/newsuk/times-components#readme",
  "devDependencies": {
    "@storybook/addon-knobs": "3.4.1",
    "@storybook/react-native": "3.4.1",
    "@times-components/eslint-config-thetimes": "0.4.0",
    "@times-components/jest-configurator": "0.5.1",
    "@times-components/storybook": "0.7.2",
    "babel-plugin-add-react-displayname": "0.0.5",
    "babel-plugin-flow-react-proptypes": "22.0.0",
    "babel-plugin-styled-components": "1.5.1",
    "babel-preset-react-native": "4.0.0",
    "dextrose": "1.7.6",
    "enzyme": "3.3.0",
    "eslint": "4.9.0",
    "jest": "21.2.1",
    "prettier": "1.8.2",
    "react": "16.2.0",
    "react-dom": "16.2.0",
    "react-native": "0.53.3",
    "react-test-renderer": "16.2.0"
  },
  "dependencies": {
    "@times-components/responsive-styles": "0.6.8",
    "@times-components/styleguide": "0.5.12",
    "prop-types": "15.6.0"
  },
  "peerDependencies": {
    "react": ">=16",
    "react-dom": ">=16",
    "react-native": ">=0.50",
    "react-native-web": ">=0.3"
  },
  "publishConfig": {
    "access": "public"
  },
  "dev": "src/slice"
}
