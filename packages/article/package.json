{
  "name": "@times-components/article",
  "version": "6.8.24",
  "description": "The article",
  "main": "dist/article",
  "dev": "src/article",
  "scripts": {
    "fmt": "eslint . --fix && prettier --write '**/*.*'",
    "prettier:diff": "prettier --list-different '**/*.*'",
    "depcheck": "depcheck --ignores='@thetimes/jest-lint,babel-*,depcheck,eslint,jest,prettier,react-art,stylelint*,webpack*' --ignore-bin-package=false --skip-missing",
    "lint": "eslint . && yarn prettier:diff && yarn depcheck && jest-lint",
    "prepublishOnly": "yarn transpile && yarn bundle",
    "test": "jest --config='./__tests__/jest.config.js'",
    "cleanup-dist": "rm -rf dist",
    "transpile": "yarn cleanup-dist && babel src -d dist",
    "bundle": "NODE_ENV=production webpack -p"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/newsuk/times-components.git"
  },
  "keywords": [
    "react-native-web",
    "react",
    "native",
    "web",
    "article",
    "component"
  ],
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/newsuk/times-components/issues"
  },
  "homepage": "https://github.com/newsuk/times-components#readme",
  "devDependencies": {
    "@babel/cli": "7.4.3",
    "@babel/core": "7.4.3",
    "@thetimes/jest-lint": "*",
    "@times-components/eslint-config-thetimes": "0.8.9",
    "@times-components/jest-configurator": "2.5.3",
    "@times-components/provider-test-tools": "1.17.4",
    "@times-components/storybook": "3.4.25",
    "@times-components/tealium-utils": "0.7.41",
    "@times-components/webpack-configurator": "2.0.15",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "24.7.1",
    "babel-loader": "8.0.5",
    "babel-plugin-add-react-displayname": "0.0.5",
    "babel-plugin-styled-components": "1.5.1",
    "metro-react-native-babel-preset": "0.53.1",
    "depcheck": "0.6.9",
    "eslint": "5.9.0",
    "jest": "24.7.1",
    "prettier": "1.14.3",
    "react": "16.5.2",
    "react-art": "16.5.2",
    "react-dom": "16.5.2",
    "react-native": "0.55.4",
    "react-native-web": "0.9.0",
    "react-test-renderer": "16.5.2",
    "webpack": "4.30.0",
    "webpack-cli": "3.3.0"
  },
  "dependencies": {
    "@times-components/ad": "2.5.28",
    "@times-components/article-in-depth": "1.2.6",
    "@times-components/article-magazine-comment": "1.3.6",
    "@times-components/article-magazine-standard": "1.2.6",
    "@times-components/article-main-comment": "0.19.6",
    "@times-components/article-main-standard": "1.2.6",
    "@times-components/context": "0.9.20",
    "@times-components/provider": "1.16.5",
    "@times-components/styleguide": "3.28.5",
    "styled-components": "3.4.0"
  },
  "peerDependencies": {
    "react": ">=16.5",
    "react-dom": ">=16.5",
    "react-native": ">=0.55",
    "react-native-web": "0.9.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
