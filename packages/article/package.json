{
  "name": "@times-components/article",
  "version": "6.5.10",
  "description": "The article",
  "main": "dist/article",
  "dev": "src/article",
  "scripts": {
    "fmt": "eslint . --fix && prettier --write '**/*.*'",
    "prettier:diff": "prettier --list-different '**/*.*'",
    "depcheck": "depcheck --ignores='@thetimes/jest-lint,babel-*,depcheck,eslint,jest,prettier,react-art,stylelint*,webpack*' --ignore-bin-package=false --skip-missing",
    "lint": "eslint . && yarn prettier:diff && yarn depcheck && jest-lint",
    "prepublishOnly": "yarn transpile && yarn bundle",
    "test": "jest --config='./__tests__/jest.config.js'",
    "cleanup-dist": "rm -rf dist",
    "transpile": "yarn cleanup-dist && babel src -d dist",
    "bundle": "NODE_ENV=production webpack -p"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/newsuk/times-components.git"
  },
  "keywords": [
    "react-native-web",
    "react",
    "native",
    "web",
    "article",
    "component"
  ],
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/newsuk/times-components/issues"
  },
  "homepage": "https://github.com/newsuk/times-components#readme",
  "devDependencies": {
    "@thetimes/jest-lint": "*",
    "@times-components/eslint-config-thetimes": "0.8.8",
    "@times-components/jest-configurator": "2.1.18",
    "@times-components/provider-test-tools": "1.10.5",
    "@times-components/storybook": "3.2.2",
    "@times-components/tealium-utils": "0.7.9",
    "@times-components/webpack-configurator": "2.0.14",
    "babel-cli": "6.26.0",
    "babel-core": "6.26.0",
    "babel-loader": "7.1.4",
    "babel-plugin-add-react-displayname": "0.0.5",
    "babel-plugin-styled-components": "1.5.1",
    "babel-preset-react-native": "4.0.0",
    "depcheck": "0.6.9",
    "eslint": "5.9.0",
    "jest": "23.3.0",
    "prettier": "1.14.3",
    "react": "16.5.2",
    "react-art": "16.5.2",
    "react-dom": "16.5.2",
    "react-native": "0.55.4",
    "react-native-web": "0.9.0",
    "react-test-renderer": "16.5.2",
    "webpack": "4.6.0",
    "webpack-cli": "2.1.4"
  },
  "dependencies": {
    "@times-components/ad": "2.2.5",
    "@times-components/article-magazine-comment": "0.1.15",
    "@times-components/article-magazine-standard": "0.1.9",
    "@times-components/article-main-comment": "0.3.14",
    "@times-components/article-main-standard": "0.3.11",
    "@times-components/context": "0.5.16",
    "@times-components/provider": "1.8.7",
    "@times-components/styleguide": "3.9.3",
    "styled-components": "3.4.0"
  },
  "peerDependencies": {
    "react": ">=16.5",
    "react-dom": ">=16.5",
    "react-native": ">=0.55",
    "react-native-web": "0.9.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
