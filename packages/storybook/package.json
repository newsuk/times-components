{
  "name": "@times-components/storybook",
  "version": "4.0.9",
  "description": "React storybook helpers for Times Components",
  "main": "dist/storybook",
  "dev": "src/storybook",
  "scripts": {
    "fmt": "eslint . --fix && prettier --write '**/*.*'",
    "prettier:diff": "prettier --list-different '**/*.*'",
    "depcheck": "depcheck --ignores='babel-*,depcheck,eslint,jest,prettier,react-art,webpack*' --ignore-bin-package=false --skip-missing",
    "lint": "eslint . && yarn prettier:diff && yarn depcheck",
    "test:android": "jest --config='./__tests__/android/jest.config.js'",
    "test:ios": "jest --config='./__tests__/ios/jest.config.js'",
    "test:web": "jest --config='./__tests__/web/jest.config.js'",
    "test:all": "yarn test:android --coverage && yarn test:ios --coverage && yarn test:web --coverage",
    "prepublishOnly": "yarn transpile",
    "cleanup-dist": "rm -rf dist",
    "transpile": "yarn cleanup-dist && babel src -d dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/newsuk/times-components.git"
  },
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/newsuk/times-components/issues"
  },
  "homepage": "https://github.com/newsuk/times-components#readme",
  "devDependencies": {
    "@times-components/jest-configurator": "2.5.12",
    "@times-components/jest-serializer": "3.2.8",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "24.8.0",
    "babel-plugin-add-react-displayname": "0.0.5",
    "babel-plugin-styled-components": "1.10.0",
    "depcheck": "0.6.9",
    "eslint": "5.9.0",
    "jest": "24.8.0",
    "prettier": "1.14.3",
    "react-art": "16.6.3",
    "react-native-web": "0.11.4",
    "react-test-renderer": "16.6.3"
  },
  "dependencies": {
    "@storybook/addon-actions": "4.1.18",
    "@storybook/addon-knobs": "4.1.18",
    "@storybook/react-native": "4.1.18",
    "@times-components/schema": "0.6.12",
    "apollo-cache-inmemory": "1.5.1",
    "apollo-client": "2.5.1",
    "apollo-link-http": "1.5.14",
    "babel-core": "^7.0.0-bridge.0",
    "prop-types": "15.7.2",
    "react": "16.8.6",
    "react-apollo": "2.5.5",
    "react-helmet-async": "1.0.2",
    "react-test-renderer": "16.6.3"
  },
  "peerDependencies": {
    "react": ">=16.5",
    "react-dom": ">=16.5",
    "react-native": ">=0.55",
    "react-native-web": "0.11.4"
  },
  "publishConfig": {
    "access": "public"
  }
}
