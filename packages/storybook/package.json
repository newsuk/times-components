{
  "name": "@times-components/storybook",
  "version": "0.4.5",
  "description": "React storybook helpers for Times Components",
  "main": "dist/storybook",
  "dev": "src/storybook",
  "scripts": {
    "fmt": "prettier --write '**/*.*'",
    "prettier:diff": "prettier --list-different '**/*.*'",
    "lint": "eslint . && yarn prettier:diff",
    "test:android": "jest --config='./src/__tests__/android/jest.config.js'",
    "test:ios": "jest --config='./src/__tests__/ios/jest.config.js'",
    "test:web": "jest --config='./src/__tests__/web/jest.config.js'",
    "test:all": "yarn test:android --coverage & yarn test:ios --coverage & yarn test:web --coverage",
    "prepublishOnly": "yarn transpile",
    "transpile": "babel src --ignore node_modules,__tests__ -d dist",
    "postinstall": "yarn transpile"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/newsuk/times-components.git"
  },
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/newsuk/times-components/issues"
  },
  "homepage": "https://github.com/newsuk/times-components#readme",
  "devDependencies": {
    "jest": "21.2.1",
    "prettier": "1.8.2"
  },
  "dependencies": {
    "@storybook/addon-actions": "3.3.15",
    "@storybook/addons": "3.3.15",
    "@times-components/jest-configurator": "0.2.3",
    "eslint": "4.9.0",
    "eslint-config-airbnb": "16.1.0",
    "eslint-config-prettier": "2.6.0",
    "eslint-plugin-import": "2.8.0",
    "eslint-plugin-jsx-a11y": "6.0.2",
    "eslint-plugin-react": "7.4.0",
    "react": "16.2.0",
    "react-dom": "16.2.0",
    "react-native": "0.54.2",
    "react-test-renderer": "16.3.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
