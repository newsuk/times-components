{
  "name": "@times-components/storybook",
  "version": "4.7.0",
  "description": "React storybook helpers for Times Components",
  "main": "dist/storybook",
  "dev": "src/storybook",
  "scripts": {
    "fmt": "eslint . --fix && prettier --write '**/*.*'",
    "prettier:diff": "prettier --list-different '**/*.*'",
    "depcheck": "depcheck --ignores='@babel/*,babel-*,depcheck,eslint,jest,prettier,react-art,webpack*' --ignore-bin-package=false --skip-missing",
    "lint": "eslint . && yarn prettier:diff && yarn depcheck",
    "test:web": "jest --config='./__tests__/jest.config.js'",
    "test:web:updatesnapshot": "yarn test:web -u",
    "prepublishOnly": "yarn transpile",
    "cleanup-dist": "rm -rf dist",
    "transpile": "yarn cleanup-dist && babel src -d dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/newsuk/times-components.git"
  },
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/newsuk/times-components/issues"
  },
  "homepage": "https://github.com/newsuk/times-components#readme",
  "devDependencies": {
    "@babel/core": "7.4.4",
    "@times-components/jest-configurator-web": "0.8.0",
    "babel-jest": "24.8.0",
    "depcheck": "0.6.9",
    "eslint": "5.9.0",
    "jest": "24.8.0",
    "prettier": "1.14.3",
    "react-test-renderer": "16.9.0"
  },
  "dependencies": {
    "@babel/core": "7.4.4",
    "@storybook/addon-actions": "6.5.15",
    "@storybook/addon-knobs": "6.4.0",
    "@storybook/react": "6.5.15",
    "@times-components/schema": "0.7.3",
    "@times-components/user-state": "0.4.18",
    "@times-components/utils": "6.15.17",
    "apollo-cache-inmemory": "1.5.1",
    "apollo-client": "2.5.1",
    "apollo-link-http": "1.5.14",
    "prop-types": "15.7.2",
    "react": "16.9.0",
    "react-apollo": "2.5.5",
    "react-helmet-async": "1.0.2",
    "styled-components": "4.3.2"
  },
  "resolutions": {
    "react": "16.9.0",
    "react-dom": "16.9.0"
  },
  "peerDependencies": {
    "react": ">=16.9",
    "react-dom": ">=16.9"
  },
  "publishConfig": {
    "access": "public"
  }
}
