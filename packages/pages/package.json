{
  "name": "@times-components/pages",
  "version": "2.0.78",
  "main": "dist/pages",
  "dev": "src/pages",
  "scripts": {
    "fmt": "eslint . --fix && prettier --write '**/*.*'",
    "prettier:diff": "prettier --list-different '**/*.*'",
    "depcheck": "depcheck --ignores='babel-*,depcheck,eslint,graphql,jest,prettier,webpack*' --ignore-bin-package=false --skip-missing",
    "lint": "eslint . && yarn prettier:diff && yarn depcheck",
    "test:android": "jest --config='./__tests__/android/jest.config.js'",
    "test:ios": "jest --config='./__tests__/ios/jest.config.js'",
    "test:all": "yarn test:android --coverage && yarn test:ios --coverage",
    "prepublishOnly": "yarn transpile",
    "watch": "watch 'yarn prepublishOnly' ./src --ignoreDotFiles --ignoreUnreadable",
    "cleanup-dist": "rm -rf dist",
    "transpile": "yarn cleanup-dist && babel src -d dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/newsuk/times-components.git"
  },
  "keywords": [
    "react-native-web",
    "react",
    "native",
    "web",
    "pages",
    "component"
  ],
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/newsuk/times-components/issues"
  },
  "homepage": "https://github.com/newsuk/times-components#readme",
  "devDependencies": {
    "@times-components/eslint-config-thetimes": "0.8.12",
    "@times-components/jest-configurator": "2.5.12",
    "@times-components/jest-serializer": "3.2.8",
    "@times-components/storybook": "4.0.13",
    "@times-components/test-utils": "2.2.43",
    "@times-components/webpack-configurator": "2.0.21",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "24.8.0",
    "depcheck": "0.6.9",
    "eslint": "5.9.0",
    "jest": "24.8.0",
    "prettier": "1.14.3",
    "react": "16.6.3",
    "react-dom": "16.6.3",
    "react-native": "0.58.6",
    "react-test-renderer": "16.6.3",
    "webpack": "4.30.0"
  },
  "dependencies": {
    "@times-components/article": "7.2.40",
    "@times-components/author-profile": "6.0.49",
    "@times-components/context": "1.0.20",
    "@times-components/provider": "1.17.11",
    "@times-components/schema": "0.6.13",
    "@times-components/section": "1.0.35",
    "@times-components/styleguide": "3.28.26",
    "@times-components/topic": "5.0.49",
    "apollo-cache-inmemory": "1.5.1",
    "apollo-client": "2.5.1",
    "apollo-link-http": "1.5.14",
    "graphql": "14.2.1",
    "prop-types": "15.7.2",
    "react-apollo": "2.5.5"
  },
  "peerDependencies": {
    "react": ">=16",
    "react-dom": ">=16",
    "react-native": ">=0.50",
    "react-native-web": "0.11.2"
  },
  "publishConfig": {
    "access": "public"
  }
}
