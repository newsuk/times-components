    "@times-components/storybook": "4.0.34",
    "@times-components/tealium-utils": "0.7.61",
    "@times-components/test-utils": "2.3.5",
    "@times-components/webpack-configurator": "2.0.25",
    "babel-jest": "24.8.0",
    "eslint": "5.9.0",
    "jest": "24.8.0",
    "jest-styled-components": "6.3.1",
