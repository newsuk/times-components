{
  "name": "@times-components/markup-forest",
  "version": "1.7.26",
  "description": "Utility functions for traversing The Times' AST",
  "main": "dist/markup-forest",
  "dev": "src/markup-forest",
  "scripts": {
    "fmt": "eslint . --fix && prettier --write '**/*.*'",
    "prettier:diff": "prettier --list-different '**/*.*'",
    "lint": "eslint . && yarn prettier:diff",
    "test": "jest",
    "prepublishOnly": "yarn transpile && yarn bundle",
    "watch": "watch 'yarn bundle' ./src --ignoreDotFiles --ignoreUnreadable",
    "cleanup-dist": "rm -rf dist",
    "transpile": "yarn cleanup-dist && babel src -d dist",
    "bundle": "NODE_ENV=production webpack -p"
  },
  "jest": {
    "testEnvironment": "jsdom",
    "testURL": "http://localhost"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/newsuk/times-components.git"
  },
  "keywords": [
    "react-native-web",
    "react",
    "native",
    "web",
    "markup-forest",
    "component"
  ],
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/newsuk/times-components/issues"
  },
  "homepage": "https://github.com/newsuk/times-components#readme",
  "devDependencies": {
    "@babel/core": "7.4.4",
    "@times-components/eslint-config-thetimes": "0.8.15",
    "@times-components/jest-configurator": "2.6.7",
    "@times-components/test-utils": "2.3.6",
    "@times-components/webpack-configurator": "2.0.26",
    "babel-jest": "24.8.0",
    "eslint": "5.9.0",
    "jest": "24.8.0",
    "prettier": "1.14.3",
    "react": "16.9.0",
    "react-test-renderer": "16.6.3",
    "webpack": "4.30.0",
    "webpack-cli": "3.3.1"
  },
  "dependencies": {
    "prop-types": "15.7.2"
  },
  "publishConfig": {
    "access": "public"
  }
}
