{
  "name": "@times-components/drop-cap",
  "version": "0.0.1",
  "description": "Drop Cap Paragraph",
  "main": "dist/drop-cap-with-context",
  "dev": "src/drop-cap-with-context",
  "scripts": {
    "depcheck": "depcheck --ignores='babel-*,depcheck,eslint,@thetimes/jest-lint,jest,prettier,webpack*' --ignore-bin-package=false --skip-missing",
    "fmt": "eslint . --fix && prettier --write '**/*.*'",
    "prettier:diff": "prettier --list-different '**/*.*'",
    "lint": "eslint . && yarn prettier:diff && yarn depcheck && jest-lint",
    "test:android": "jest --config='./__tests__/android/jest.config.js'",
    "test:all": "yarn test:android --coverage",
    "prepublishOnly": "yarn transpile && yarn bundle",
    "cleanup-dist": "rm -rf dist",
    "transpile": "yarn cleanup-dist && babel src -d dist",
    "bundle": "NODE_ENV=production webpack -p"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/newsuk/times-components.git"
  },
  "keywords": [
    "react-native-web",
    "react",
    "native",
    "web",
    "drop-cap",
    "component"
  ],
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/newsuk/times-components/issues"
  },
  "homepage": "https://github.com/newsuk/times-components#readme",
  "dependencies": {
    "prop-types": "15.6.2",
    "@times-components/context": "0.4.30",
    "@times-components/styleguide": "3.5.0",
    "lodash.invert": "4.3.0"
  },
  "devDependencies": {
    "@thetimes/jest-lint": "*",
    "@times-components/eslint-config-thetimes": "0.8.0",
    "@times-components/jest-configurator": "2.1.8",
    "@times-components/jest-serializer": "3.1.2",
    "@times-components/storybook": "3.1.15",
    "@times-components/webpack-configurator": "2.0.6",
    "babel-cli": "6.26.0",
    "eslint": "4.9.0",
    "jest": "23.3.0",
    "prettier": "1.8.2",
    "react": "16.5.2",
    "react-dom": "16.5.2",
    "react-native": "0.55.4",
    "react-test-renderer": "16.5.2",
    "webpack": "4.6.0",
    "webpack-cli": "2.1.4"
  },
  "peerDependencies": {
    "react": ">=16.5",
    "react-dom": ">=16.5",
    "react-native": ">=0.55",
    "react-native-web": "0.9.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
