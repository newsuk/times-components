{
  "name": "@times-components-native/context",
  "version": "1.2.17",
  "private": true,
  "description": "Shared context between components",
  "main": "dist/context",
  "dev": "src/context",
  "scripts": {
    "fmt": "eslint . --fix && prettier --write '**/*.*'",
    "prettier:diff": "prettier --list-different '**/*.*'",
    "lint": "eslint . && yarn prettier:diff && yarn depcheck",
    "depcheck": "depcheck --ignores='@babel/*,babel-*,depcheck,eslint,jest,prettier,webpack*' --ignore-bin-package=false --skip-missing",
    "test:android": "jest --config='./__tests__/android/jest.config.js'",
    "test:ios": "jest --config='./__tests__/ios/jest.config.js'",
    "test:all": "yarn test:android --coverage && yarn test:ios --coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/newsuk/times-components.git"
  },
  "keywords": [
    "react",
    "native",
    "context",
    "component"
  ],
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/newsuk/times-components/issues"
  },
  "homepage": "https://github.com/newsuk/times-components#readme",
  "dependencies": {
    "@times-components-native/styleguide": "3.38.10"
  },
  "devDependencies": {
    "@babel/core": "7.4.4",
    "@times-components-native/eslint-config-thetimes": "0.8.16",
    "@times-components-native/jest-configurator": "2.6.11",
    "babel-jest": "24.8.0",
    "depcheck": "0.6.9",
    "eslint": "5.9.0",
    "jest": "24.8.0",
    "prettier": "1.14.3",
    "react": "16.9.0",
    "react-dom": "16.9.0",
    "react-native": "0.61.5",
    "react-test-renderer": "16.9.0"
  },
  "resolutions": {
    "react": "16.9.0",
    "react-dom": "16.9.0"
  },
  "peerDependencies": {
    "react": ">=16",
    "react-dom": ">=16",
    "react-native": ">=0.50",
  },
  "publishConfig": {
    "access": "public"
  }
}
