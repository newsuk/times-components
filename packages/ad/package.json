{
  "name": "@times-components/ad",
  "version": "0.10.0",
  "description": "Ad component",
  "main": "ad",
  "scripts": {
    "fmt": "prettier --write '**/*.*'",
    "prettier:diff": "prettier --list-different '**/*.*'",
    "lint": "eslint . && yarn prettier:diff",
    "test": "jest",
    "dextrose-stories": "dextrose generate-stories $(pwd)",
    "dextrose-clean": "dextrose clean-stories $(pwd)"
  },
  "jest": {
    "preset": "react-native",
    "rootDir": "../../",
    "coverageDirectory": "<rootDir>/packages/ad/coverage/",
    "transformIgnorePatterns": [
      "node_modules/(?!(react-native|@times-components)/)"
    ],
    "testMatch": [
      "<rootDir>/packages/ad/__tests__/**.test.js"
    ],
    "testEnvironment": "jsdom"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/newsuk/times-components.git"
  },
  "keywords": [
    "react-native-web",
    "react",
    "native",
    "web",
    "gpt",
    "component"
  ],
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/newsuk/times-components/issues"
  },
  "homepage": "https://github.com/newsuk/times-components#readme",
  "devDependencies": {
    "@storybook/addon-actions": "3.3.11",
    "@storybook/react-native": "3.3.11",
    "@times-components/eslint-config-thetimes": "0.1.3",
    "@times-components/utils": "0.5.1",
    "dextrose": "^1.4.6",
    "enzyme": "3.3.0",
    "enzyme-adapter-react-16": "1.1.0",
    "eslint": "4.9.0",
    "jest": "21.2.1",
    "jsdom": "^9.12.0",
    "prettier": "1.8.2",
    "react": "16.2.0",
    "react-dom": "16.2.0",
    "react-native": "0.50.1",
    "react-test-renderer": "16.2.0"
  },
  "dependencies": {
    "@times-components/watermark": "0.3.7",
    "prop-types": "15.6.0",
    "react-broadcast": "0.5.2"
  },
  "peerDependencies": {
    "babel-preset-env": "1.6.1",
    "babel-preset-stage-2": "6.24.1",
    "react": ">=16",
    "react-dom": ">=16",
    "react-native": ">=0.50",
    "react-native-web": ">=0.3"
  },
  "publishConfig": {
    "access": "public"
  }
}
