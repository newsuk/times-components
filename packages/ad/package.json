{
  "name": "@times-components/ad",
  "version": "2.21.15",
  "description": "Ad component",
  "main": "dist/ad",
  "dev": "src/ad",
  "scripts": {
    "prepublishOnly": "yarn transpile && yarn bundle",
    "fmt": "eslint . --fix && prettier --write '**/*.*'",
    "prettier:diff": "prettier --list-different '**/*.*'",
    "depcheck": "depcheck --ignores='@babel/*,babel-*,depcheck,eslint,jest,prettier,webpack*' --ignore-bin-package=false --skip-missing",
    "lint": "eslint . && yarn prettier:diff && yarn depcheck",
    "test:web": "jest --config='./__tests__/jest.config.js'",
    "test:web:updatesnapshot": "yarn test:web -u",
    "watch": "watch 'yarn bundle' ./src --ignoreDotFiles --ignoreUnreadable",
    "cleanup-dist": "rm -rf dist",
    "transpile": "yarn cleanup-dist && babel src -d dist",
    "bundle": "NODE_ENV=production webpack -p"
  },
  "keywords": [
    "react",
    "gpt",
    "component"
  ],
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/newsuk/times-components/issues"
  },
  "homepage": "https://github.com/newsuk/times-components#readme",
  "devDependencies": {
    "@babel/core": "7.4.4",
    "@times-components/eslint-config-thetimes": "0.8.18",
    "@times-components/jest-configurator-web": "0.8.0",
    "@times-components/jest-serializer": "3.5.5",
    "@times-components/storybook": "4.5.10",
    "@times-components/test-utils": "2.4.0",
    "@times-components/webpack-configurator": "2.1.0",
    "babel-jest": "24.8.0",
    "babel-loader": "8.0.5",
    "babel-plugin-transform-class-properties": "6.24.1",
    "depcheck": "0.6.9",
    "enzyme": "3.9.0",
    "eslint": "5.9.0",
    "jest": "24.8.0",
    "jsdom": "9.12.0",
    "lodash.merge": "4.6.2",
    "prettier": "1.14.3",
    "react": "16.9.0",
    "react-dom": "16.9.0",
    "react-test-renderer": "16.9.0",
    "webpack": "4.30.0"
  },
  "dependencies": {
    "@times-components/ts-styleguide": "1.39.2",
    "@times-components/utils": "6.15.14",
    "prop-types": "15.7.2",
    "react-broadcast": "0.5.2"
  },
  "resolutions": {
    "react": "16.9.0",
    "react-dom": "16.9.0"
  },
  "peerDependencies": {
    "react": ">=16.9",
    "react-dom": ">=16.9"
  },
  "publishConfig": {
    "access": "public"
  }
}
