{
  "name": "@times-components/ad",
  "version": "2.6.1",
  "description": "Ad component",
  "main": "dist/ad",
  "dev": "src/ad",
  "scripts": {
    "prepublishOnly": "yarn transpile && yarn bundle",
    "fmt": "eslint . --fix && prettier --write '**/*.*'",
    "prettier:diff": "prettier --list-different '**/*.*'",
    "depcheck": "depcheck --ignores='babel-*,depcheck,eslint,jest,prettier,webpack*' --ignore-bin-package=false --skip-missing",
    "lint": "eslint . && yarn prettier:diff && yarn depcheck",
    "test:android": "jest --config='./__tests__/android/jest.config.js'",
    "test:ios": "jest --config='./__tests__/ios/jest.config.js'",
    "test:web": "jest --config='./__tests__/web/jest.config.js'",
    "test:all": "yarn test:android --coverage && yarn test:ios --coverage && yarn test:web --coverage",
    "cleanup-dist": "rm -rf dist",
    "transpile": "yarn cleanup-dist && babel src -d dist",
    "bundle": "NODE_ENV=production webpack -p"
  },
  "keywords": [
    "react-native-web",
    "react",
    "native",
    "web",
    "gpt",
    "component"
  ],
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/newsuk/times-components/issues"
  },
  "homepage": "https://github.com/newsuk/times-components#readme",
  "devDependencies": {
    "@times-components/eslint-config-thetimes": "0.8.11",
    "@times-components/jest-configurator": "2.5.9",
    "@times-components/jest-serializer": "3.2.5",
    "@times-components/storybook": "4.0.6",
    "@times-components/test-utils": "2.2.41",
    "@times-components/webpack-configurator": "2.0.20",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "24.8.0",
    "babel-loader": "8.0.5",
    "babel-plugin-add-react-displayname": "0.0.5",
    "babel-plugin-styled-components": "1.10.0",
    "depcheck": "0.6.9",
    "enzyme": "3.9.0",
    "eslint": "5.9.0",
    "jest": "24.8.0",
    "jsdom": "9.12.0",
    "lodash.merge": "4.6.0",
    "prettier": "1.14.3",
    "react": "16.6.3",
    "react-dom": "16.6.3",
    "react-native": "0.58.6",
    "react-test-renderer": "16.6.3",
    "webpack": "4.30.0",
    "webpack-cli": "3.3.1"
  },
  "dependencies": {
    "@times-components/styleguide": "3.28.15",
    "@times-components/utils": "4.11.1",
    "@times-components/watermark": "2.6.12",
    "prop-types": "15.7.2",
    "react-broadcast": "0.5.2"
  },
  "peerDependencies": {
    "react": ">=16.5",
    "react-dom": ">=16.5",
    "react-native": ">=0.55",
    "react-native-web": "0.11.2"
  },
  "publishConfig": {
    "access": "public"
  }
}
