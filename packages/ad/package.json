{
  "name": "@times-components/ad",
  "version": "0.21.8",
  "description": "Ad component",
  "main": "dist/ad",
  "dev": "src/ad",
  "scripts": {
    "prepublishOnly": "yarn transpile && yarn bundle",
    "fmt": "prettier --write '**/*.*'",
    "prettier:diff": "prettier --list-different '**/*.*'",
    "lint": "eslint . && yarn prettier:diff",
    "test": "jest",
    "cleanup-dist": "rm -rf dist",
    "transpile": "yarn cleanup-dist && babel src -d dist",
    "bundle": "NODE_ENV=production webpack -p"
  },
  "jest": {
    "preset": "react-native",
    "rootDir": "../../",
    "coverageDirectory": "<rootDir>/packages/ad/coverage/",
    "transformIgnorePatterns": [
      "node_modules/(?!(react-native|@times-components)/)"
    ],
    "testMatch": [
      "<rootDir>/packages/ad/__tests__/**.test.js"
    ],
    "testEnvironment": "jsdom"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/newsuk/times-components.git"
  },
  "keywords": [
    "react-native-web",
    "react",
    "native",
    "web",
    "gpt",
    "component"
  ],
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/newsuk/times-components/issues"
  },
  "homepage": "https://github.com/newsuk/times-components#readme",
  "devDependencies": {
    "@times-components/eslint-config-thetimes": "0.6.1",
    "@times-components/storybook": "0.13.3",
    "@times-components/webpack-configurator": "0.1.1",
    "babel-core": "6.26.0",
    "babel-loader": "7.1.4",
    "babel-plugin-add-react-displayname": "0.0.5",
    "babel-plugin-flow-react-proptypes": "22.0.0",
    "babel-plugin-react-native-web": "0.6.1",
    "babel-plugin-styled-components": "1.5.1",
    "babel-preset-react-native": "4.0.0",
    "enzyme": "3.3.0",
    "enzyme-adapter-react-16": "1.1.0",
    "eslint": "4.9.0",
    "jest": "21.2.1",
    "jsdom": "9.12.0",
    "lodash.merge": "4.6.0",
    "prettier": "1.8.2",
    "react": "16.3.1",
    "react-dom": "16.3.1",
    "react-native": "0.54.2",
    "react-test-renderer": "16.3.1",
    "webpack": "4.6.0",
    "webpack-cli": "2.1.4"
  },
  "dependencies": {
    "@times-components/styleguide": "0.11.1",
    "@times-components/utils": "0.15.0",
    "@times-components/watermark": "0.7.9",
    "prop-types": "15.6.0",
    "react-broadcast": "0.5.2"
  },
  "peerDependencies": {
    "react": ">=16.2",
    "react-dom": ">=16.2",
    "react-native": ">=0.54",
    "react-native-web": ">=0.3"
  },
  "publishConfig": {
    "access": "public"
  }
}
