{
  "name": "@times-components-native/video",
  "version": "4.10.18",
  "private": true,
  "main": "dist/video",
  "dev": "src/video",
  "scripts": {
    "fmt": "eslint . --fix && prettier --write '**/*.*'",
    "prettier:diff": "prettier --list-different '**/*.*'",
    "depcheck": "depcheck --ignores='@babel/*,babel-*,depcheck,eslint,jest,prettier,react-art,webpack*' --ignore-bin-package=false --skip-missing",
    "lint": "eslint . && yarn prettier:diff && yarn depcheck",
    "test:ios": "jest --config='./__tests__/ios/jest.config.js'",
    "test:android": "jest --config='./__tests__/android/jest.config.js'",
    "test:all": "yarn test:ios --coverage && yarn test:android --coverage && yarn test:web --coverage"
  },
  "jest": {
    "preset": "react-native",
    "rootDir": "../..",
    "coverageDirectory": "<rootDir>/packages/video/coverage/",
    "testEnvironment": "jsdom",
    "transformIgnorePatterns": [
      "node_modules/(?!(react-native|@times-components)/)"
    ],
    "testMatch": [
      "<rootDir>/packages/video/__tests__/*.test.js"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/newsuk/times-components.git"
  },
  "keywords": [
    "react",
    "native",
    "video",
    "brightcove",
    "component"
  ],
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/newsuk/times-components/issues"
  },
  "homepage": "https://github.com/newsuk/times-components#readme",
  "devDependencies": {
    "@babel/core": "7.4.4",
    "@times-components-native/eslint-config-thetimes": "0.8.16",
    "@times-components-native/jest-configurator": "2.6.11",
    "@times-components-native/jest-serializer": "3.2.23",
    "@times-components-native/storybook": "4.1.54",
    "@times-components-native/test-utils": "2.3.8",
    "babel-jest": "24.8.0",
    "babel-loader": "8.0.5",
    "babel-plugin-add-react-displayname": "0.0.5",
    "babel-plugin-styled-components": "1.10.6",
    "depcheck": "0.6.9",
    "enzyme": "3.9.0",
    "eslint": "5.9.0",
    "jest": "24.8.0",
    "prettier": "1.14.3",
    "react": "16.9.0",
    "react-art": "16.6.3",
    "react-dom": "16.9.0",
    "react-native": "0.61.5",
    "react-test-renderer": "16.9.0"
  },
  "dependencies": {
    "@times-components-native/error-view": "2.3.48",
    "@times-components-native/icons": "2.14.49",
    "@times-components-native/image": "6.7.25",
    "@times-components-native/styleguide": "3.38.10",
    "@times-components-native/svgs": "2.7.30",
    "@times-components-native/utils": "6.3.14",
    "prop-types": "15.7.2",
    "styled-components": "4.3.2"
  },
  "resolutions": {
    "react": "16.9.0",
    "react-dom": "16.9.0"
  },
  "peerDependencies": {
    "react": ">=16.9",
    "react-dom": ">=16.9",
    "react-native": ">=0.59"
  },
  "_timesComponentsCliVariables": {
    "component": "Video",
    "packageDescription": "Video component",
    "packageName": "video"
  },
  "publishConfig": {
    "access": "public"
  }
}
