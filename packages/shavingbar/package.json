{
  "name": "@times-components/shavingbar",
  "version": "0.0.1",
  "description": "A fancy bar for saving & sharing articles",
  "main": "dist",
  "dev": "src",
  "scripts": {
    "fmt": "prettier --write '**/*.*'",
    "prettier:diff": "prettier --list-different '**/*.*'",
    "lint": "eslint . && npm run prettier:diff",
    "test:web": "jest --config='./__tests__/web/jest.config.js'",
    "test:all": "yarn test:web --coverage",
    "dextrose-stories": "dextrose generate-stories $(pwd)",
    "dextrose-clean": "dextrose clean-stories $(pwd)",
    "prepublishOnly": "npm run transpile",
    "cleanup-dist": "rm -rf dist",
    "transpile": "yarn cleanup-dist && babel src -d dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/newsuk/times-components.git"
  },
  "keywords": [
    "react-native-web",
    "react",
    "native",
    "web",
    "shavingbar",
    "component"
  ],
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/newsuk/times-components/issues"
  },
  "homepage": "https://github.com/newsuk/times-components#readme",
  "devDependencies": {
    "@storybook/addon-actions": "3.4.1",
    "@storybook/addon-knobs": "3.4.1",
    "@storybook/react-native": "3.4.1",
    "@times-components/eslint-config-thetimes": "0.4.1",
    "@times-components/jest-configurator": "0.6.1",
    "@times-components/styleguide": "0.6.1",
    "babel-cli": "6.26.0",
    "enzyme": "3.3.0",
    "eslint": "4.9.0",
    "jest": "21.2.1",
    "prettier": "1.8.2",
    "react": "16.3.1",
    "react-dom": "16.3.1",
    "react-native": "0.54.2",
    "react-test-renderer": "16.3.1"
  },
  "dependencies": {
    "@times-components/icons": "0.7.2",
    "@times-components/responsive-styles": "0.7.1",
    "@times-components/styleguide": "0.6.1",
    "prop-types": "15.6.0"
  },
  "peerDependencies": {
    "react": ">=16",
    "react-dom": ">=16",
    "react-native": ">=0.50",
    "react-native-web": ">=0.3"
  },
  "publishConfig": {
    "access": "public"
  }
}
