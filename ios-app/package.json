{
  "name": "@times-components/ios-app",
  "version": "0.15.38",
  "license": "BSD-3-Clause",
  "scripts": {
    "build": "cat package.json | grep version | head -1 | sed 's/[\",\t ]//g' | awk -F: '{ print \"Bundle Version: \" $2 }' > ios-assets/js/version.meta",
    "fmt": "prettier --write '**/*.*'",
    "prettier:diff": "prettier --list-different '**/*.*'",
    "lint": "eslint . && yarn prettier:diff",
    "start": "react-native start --config metro.config.js"
  },
  "dependencies": {
    "@times-components/pages": "2.0.162",
    "@times-components/text-flow": "0.2.8",
    "prop-types": "15.7.2",
    "react": "16.8.6",
    "react-native": "0.59.10",
    "url-polyfill": "1.1.0"
  },
  "devDependencies": {
    "metro-react-native-babel-preset": "0.53.1",
    "prettier": "1.14.3",
    "webpack": "4.30.0"
  },
  "private": true
}
