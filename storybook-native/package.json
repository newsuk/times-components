{
  "name": "@times-components/storybook-native",
  "version": "0.2.10",
  "license": "BSD-3-Clause",
  "scripts": {
    "fmt": "prettier --write '**/*.*'",
    "prettier:diff": "prettier --list-different '**/*.*'",
    "lint": "eslint . && yarn prettier:diff",
    "postinstall": "rnstl",
    "start": "react-native start --config metro.config.js"
  },
  "config": {
    "react-native-storybook-loader": {
      "searchDir": [
        "../packages"
      ],
      "pattern": "!(brightcove-video)/*.stories!(.web).js",
      "outputFile": "./story-loader.js"
    }
  },
  "dependencies": {
    "@storybook/addon-actions": "4.1.18",
    "@storybook/addon-info": "4.1.18",
    "@storybook/addon-knobs": "4.1.18",
    "@storybook/addon-ondevice-knobs": "4.1.18",
    "@storybook/addon-options": "4.1.18",
    "@storybook/addons": "4.1.18",
    "@storybook/react": "4.1.18",
    "@storybook/react-native": "4.1.18",
    "@times-components/storybook": "4.1.51",
    "core-js": "^2.6.5",
    "prop-types": "15.7.2",
    "react": "16.9.0",
    "react-native": "0.61.1",
    "react-native-svg": "9.11.1",
    "regenerator-runtime": "0.13.2",
    "url-polyfill": "1.1.0"
  },
  "devDependencies": {
    "metro-react-native-babel-preset": "0.53.1",
    "prettier": "1.14.3",
    "react-native-storybook-loader": "1.8.0",
    "webpack": "4.30.0"
  },
  "private": true
}
