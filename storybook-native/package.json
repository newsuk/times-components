{
  "name": "@times-components/storybook-native",
  "version": "0.0.5",
  "license": "BSD-3-Clause",
  "scripts": {
    "fmt": "prettier --write '**/*.*'",
    "prettier:diff": "prettier --list-different '**/*.*'",
    "lint": "eslint . && yarn prettier:diff",
    "postinstall": "rnstl",
    "start": "react-native start --config ../../../../storybook-native/metro.config.js"
  },
  "config": {
    "react-native-storybook-loader": {
      "searchDir": [
        "../packages"
      ],
      "pattern": "!(brightcove-video)/*.stories!(.web).js",
      "outputFile": "./story-loader.js"
    }
  },
  "dependencies": {
    "@storybook/addon-actions": "4.1.18",
    "@storybook/addon-info": "4.1.18",
    "@storybook/addon-knobs": "4.1.18",
    "@storybook/addon-options": "4.1.18",
    "@storybook/addons": "4.1.18",
    "@storybook/react": "4.1.18",
    "@storybook/react-native": "4.1.18",
    "@times-components/storybook": "4.0.9",
    "babel-polyfill": "6.26.0",
    "prop-types": "15.7.2",
    "react": "16.8.6",
    "react-native": "0.59.8",
    "react-native-svg": "9.4.0",
    "url-polyfill": "1.1.0"
  },
  "devDependencies": {
    "metro-react-native-babel-preset": "0.53.1",
    "prettier": "1.14.3",
    "react-native-storybook-loader": "1.8.0",
    "webpack": "4.30.0"
  },
  "private": true
}
